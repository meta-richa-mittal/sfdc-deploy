<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Object and searchable fields configurations are to be stored here</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Display_Field__c</fullName>
        <defaultValue>&quot;Name&quot;</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>Field API name for which the field value will be displayed as auto-complete suggestion</inlineHelpText>
        <label>Display Field for Auto-Complete</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Display_Info__c</fullName>
        <description>Additional information of the search result to display in search suggestions. Object field name should be inserted in format in braces like: {Name} e.g. Unit {Name} {Qty__c}</description>
        <externalId>false</externalId>
        <inlineHelpText>Additional information of the search result to display in search suggestions. Object field name should be inserted in format in braces like: {Name} e.g. Unit {Name} {Qty__c}</inlineHelpText>
        <label>Display Info</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>External_Id__c</fullName>
        <caseSensitive>false</caseSensitive>
        <externalId>true</externalId>
        <label>External Id</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Object_Name__c</fullName>
        <description>Object name for which the setting applies</description>
        <externalId>false</externalId>
        <inlineHelpText>Object API Name (In case Customer and Vendor, provide Customer and Vendor respectively)</inlineHelpText>
        <label>Object Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Order__c</fullName>
        <defaultValue>1</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>Preferred order number of the object records to be displayed in search suggestions. If found conflict then ordered by object name</inlineHelpText>
        <label>Order</label>
        <precision>2</precision>
        <required>true</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Searchable_Field__c</fullName>
        <defaultValue>&quot;Name&quot;</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>Searchable field API name for which auto-complete search text will perform search. (Only 1 Field)</inlineHelpText>
        <label>Searchable Field for Auto-Complete</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>SearchConfigurations</label>
    <visibility>Protected</visibility>
</CustomObject>
