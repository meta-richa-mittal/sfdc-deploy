        <fullName>Profilt_Percentage__c</fullName>
        <description>Profilt Percentage  = (Total Price - Total Cost)/Total Price

This field is not be used</description>
        <externalId>false</externalId>
        <formula>IF(
            OR(ISNULL(Total_Price__c ), Total_Price__c = 0),
            IF(Profit_Amount__c = 0, 0, -1),
            (Profit_Amount__c / IF($Setup.Configurations__c.Tax_Included_Pricing__c,
            (Total_Price__c / (1 + Customer_Owned_Unit__r.Applicable_Tax__r.Rate__c)),
            Total_Price__c))
            )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Profilt Percentage</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Profit_Amount__c</fullName>
        <description>Profit amount is difference of total price and total cost</description>
        <externalId>false</externalId>
        <formula>IF( $Setup.Configurations__c.Tax_Included_Pricing__c , 
(( Total_Price__c / ( 1 + Customer_Owned_Unit__r.Applicable_Tax__r.Rate__c ) ) - Total_Cost__c), 
(Total_Price__c - Total_Cost__c) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Profit Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
