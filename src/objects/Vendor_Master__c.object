    <webLinks>
        <fullName>Subscribe</fullName>
        <availability>online</availability>
        <description>Used to subscribe the provided Partner Vendor Master Identifier</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Subscribe</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/56.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/56.0/apex.js&quot;)}

var apexClassRec = sforce.connection.query(&quot;SELECT NamespacePrefix FROM ApexClass where Name Like &apos;%Constants&apos;&quot;);
var nameSpace = apexClassRec.getArray(&quot;records&quot;)[0].NamespacePrefix ;

if(nameSpace != &quot;&quot; &amp;&amp; nameSpace != null &amp;&amp; nameSpace != undefined) {
    nameSpace += &quot;/&quot;;
} else {
    nameSpace = &quot;&quot;;
}

var partnerVMName = &quot;{!Vendor_Master__c.Partner_Vendor_Master_Identifier__c}&quot;;
if(partnerVMName != null &amp;&amp; partnerVMName != &quot;&quot; &amp;&amp; partnerVMName != undefined) {
    var hasError = sforce.apex.execute(nameSpace + &quot;VendorMasterSubscriberCtrl&quot;,&quot;subscribeAction&quot;,{vendorMasterName:partnerVMName});
    if(hasError == &apos;true&apos;) {
		alert(&apos;Some error occurred. Please see error logs for more detail&apos;);
	} else {
		alert(&apos;Subscribe action completed. Click OK button to refresh the page.&apos;);
		window.location.reload();
	}
} else {
	alert(&quot;Partner Vendor Master Identifier is required&quot;);
}</url>
    </webLinks>
    <webLinks>
        <fullName>Unsubscribe_and_delete</fullName>
        <availability>online</availability>
        <description>It will unsubscribe the price file and after that will delete the VM record.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Unsubscribe and delete</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/56.0/connection.js&quot;)}
    {!REQUIRESCRIPT(&quot;/soap/ajax/56.0/apex.js&quot;)}

    var apexClassRec = sforce.connection.query(&quot;SELECT NamespacePrefix FROM ApexClass where Name Like &apos;%Constants&apos;&quot;);
    var nameSpace = apexClassRec.getArray(&quot;records&quot;)[0].NamespacePrefix ;

    if(nameSpace != &quot;&quot; &amp;&amp; nameSpace != null &amp;&amp; nameSpace != undefined) {
        nameSpace += &quot;/&quot;;
    } else {
        nameSpace = &quot;&quot;;
    }

    var partnerVMName = &quot;{!Vendor_Master__c.Partner_Vendor_Master_Identifier__c}&quot;;
    var vmId = &quot;{!Vendor_Master__c.Id}&quot;;
    if(partnerVMName != null &amp;&amp; partnerVMName != &quot;&quot; &amp;&amp; partnerVMName != undefined) {
        var hasError = sforce.apex.execute(nameSpace + &quot;VendorMasterSubscriberCtrl&quot;,&quot;unsubscribeAndDeleteAction&quot;,{vendorMasterId:vmId, partnerVendorMasterName:partnerVMName});
        if(hasError == &apos;true&apos;) {
            alert(&apos;Some error occurred. Please see error logs for more detail&apos;);
        } else {
            alert(&apos;Unsubscribed and deleted successfully.&apos;);
            window.open(&apos;/&apos; + vmId.substring(0, 3),&apos;_self&apos;);
        }
    } else {
        alert(&quot;Partner Vendor Master Identifier is required&quot;);
    }</url>
    </webLinks>
