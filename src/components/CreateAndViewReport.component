<apex:component controller="GlobalController">
    <div ng-cloak="ng-cloak">
       <div class="Container-fluid">
            <div class="row M0 back-to-reporting">
                <div ng-click="HomeSearchGrid.setCreateAndViewReport(false)">
                    <i class="bp-icons bp-font-14 bp-grey-font">arrowLeft</i>
                    <p>Back to Reporting</p>
                </div>
            </div>
           <div class="row M0"> 
                   <!-- center Div start here -->
                   <div class="col-lg-10 col-md-9 col-sm-9 contentDiv">
                       <div>
                           <div class="row M0 top_row">
                               <div class="col-md-9 col-sm-10 homeSearchTopBar">
                                   <div class="homeSearchTopBarContaint row">  
                                       <div class="col-md-9 col-sm-12">
                                           <img class="starimg pull-left" src="{!Application_Images}/star.png" 
                                               ng-show="HomeSearchGrid.currentActiveFilter.Type == 'favorite'"
                                               ng-click="HomeSearchGrid.markFilterFav(HomeSearchGrid.currentActiveFilter.Id, false)"/>
                                           <i class="fa fa-star pull-left"
                                               ng-show="HomeSearchGrid.currentActiveFilter.Type != 'favorite' && HomeSearchGrid.isFilterMatched"
                                               ng-click="HomeSearchGrid.markFilterFav(HomeSearchGrid.currentActiveFilter.Id, true)"></i>
                                           <p class="T3 text-uppercase pull-left" ng-show="!HomeSearchGrid.isFilterMatched">{{ 'Label_Search' | translate }}</p>
                                           <p class="T3 text-uppercase pull-left" ng-show="HomeSearchGrid.isFilterMatched">{{HomeSearchGrid.currentActiveFilter.Name}}</p>
                                           <i class="fa fa-question-circle pull-left" data-original-title="Search" data-toggle="tooltip"
                                               data-placement="right"></i>
                                           <div class="col-md-3 col-sm-3 homeSearchTopSaveBtn"> 
                                           <button class="btn C11 T4 active_btn" ng-click="HomeSearchGrid.saveAsAction()"
                                               ng-class="{'grayBtn': HomeSearchGrid.isFilterMatched, 'addmoreinfoBtn' : !HomeSearchGrid.isFilterMatched} " 
                                               ng-if="!HomeSearchGrid.isFilterMatched"
                                               style="margin: auto; width: 80%;" tabindex="112">{{ 'Save_As' | translate }}</button>
                                           </div>
                                           <p class="homeSearchcriteriaerror T3"
                                               ng-show="!HomeSearchGrid.isFilterMatched"> {{ 'New_Search_criteria_has_not_been_saved' | translate }}</p>
                                       </div> 
                                       
                                   </div>
                               
                               </div>
       
                               <div class="rightHeaderIcon pull-right col-md-3 col-sm-2">
                                           <div class="pull-right P0  homeSearchTopBarIcon">
                                               <i class="fa fa-pie-chart" ng-click = "HomeSearchGrid.showSummarySidePanel()"></i>
                                               <i class="fa fa-print" ng-click = "HomeSearchGrid.showExportSidePanel()"></i>
                                           </div>
                                       </div>
                                  
       
                           </div>
                           <!-- filter Div start here -->
                           <div class="row M0 filter_row">
                                   <div  ng-class="{'searchBoxMultiselectWrappereNew': HomeSearchGrid.isSidePanelToggle, 'searchBoxMultiselectWrappereNewDiv' : !HomeSearchGrid.isSidePanelToggle} ">
                                       <div class="searchBoxMultiselectWrappere pull-left">
                                           <div class="searchBoxMultiselect pull-left" ng-click="HomeSearchGrid.showFilterForm()">
                                               <div class="searchCriteriaWrapper"
                                                   ng-mouseover="showMore = false" ng-mouseleave="showMore = true">
                                                   <div class="searchCriteria pull-left" ng-repeat="filter in HomeSearchGrid.searchCriteriaJson">
                                                       <label class="T3 pull-left" ng-if="filter.Label != undefined && filter.Label != null && filter.Label != ''">{{filter.Label}}:</label>
                                                       <span class="T7 filterValue" data-original-title="{{HomeSearchGrid.getFilterValue(filter.Value)}}" data-toggle="tooltip" ng-bind="HomeSearchGrid.getFilterValue(filter.Value)">
                                                       </span>
                                                       <span class="crossIcon" ng-click="HomeSearchGrid.removeFilterCriteria($event, filter.Key, $index)">&#10006;</span>   
                                                   </div>
                                               </div>
                                               <span class="moreState" ng-show="HomeSearchGrid.isDisplayMore() && showMore">{{ 'More' | translate }} ...</span>
                                               <span class="pull-right dropdownicon">
                                                   <i class="fa fa-caret-down"></i>
                                               </span>
                                           </div>
                                           <div class="pull-left searchBoxIcondiv">
                                               <i class="fa fa-search"></i>
                   
                   
                                           </div>
                                           <c:HomeSearchFilterForm />
                                       </div>
                                       <div class="sortingDiv" ng-if = "!HomeSearchGrid.isLoadGridData">
                                           <span class="T3 pull-left" style="margin-top: 3px;">{{ 'Sorting' | translate }}:</span>
                                           <div class="pull-left">
                                               <div class="sorting-badge pull-left" ng-repeat="sort in HomeSearchGrid.searchResultPageSortAttrsJSON.Sorting">
                                                   <label>
                                                       <i class="fa fa-chevron-up" ng-if="sort.SortDirection == 'ASC'"></i>
                                                       <i class="fa fa-chevron-down" ng-if="sort.SortDirection == 'DESC'"></i>
                                                   </label>  
                                                   <span class="T7" style="font-size:12px; color: #fff;">
                                                       <span ng-if="HomeSearchGrid.LabelChangeMap[sort.FieldLabel]">{{HomeSearchGrid.LabelChangeMap[sort.FieldLabel]}}</span>
                                                       <span ng-if="!HomeSearchGrid.LabelChangeMap[sort.FieldLabel]">{{sort.FieldLabel}}</span>
                                                   </span>
                                                   <span class="crossIcon" ng-click="HomeSearchGrid.removeSortCriteria($index)">&#10006;</span>   
                                               </div>
                                           </div>
                                       </div>
                                   </div>
                                   <div class="extraBackDropDiv" ng-class="{'extraBackDropDivHide': HomeSearchGrid.isSidePanelToggle, 'extraBackDropDiv' : !HomeSearchGrid.isSidePanelToggle} "></div>
                                   
                                   <!-- Side Panel Div start here -->
                                   <div class="rightSidePanel"  ng-class="{'summaryDivShow': HomeSearchGrid.isSidePanelToggle, 'summaryDivHide' : !HomeSearchGrid.isSidePanelToggle} " >
                                       <div class = "summaryInner" ng-show = "HomeSearchGrid.isSidePanelToggle">
                                        <div class = "summaryWrapper" ng-show = "HomeSearchGrid.summaryDiv">
                                           <div  class="summaryHeader col-md-12 col-sm-12">
                                               <p class="col-md-12 col-sm-12 P0 BBW">
                                                   <i class="fa fa-pie-chart F25 color727983 pull-left summaryIcon"></i>
                                                   <span class="summaryHeadding T3 pull-left">{{ 'Summary_Total' | translate }}</span>
                                                   <i class="fa fa-times pull-right color727983 CP F20 PB5 closeSummary" aria-hidden="true" 
                                                       ng-click = "HomeSearchGrid.hideSidePanel()"></i>
                                                   <!-- <i class="fa fa-cloud-upload pull-right color727983 CP F20 PB5 closeSummary" aria-hidden="true" 
                                                       ng-show="HomeSearchGrid.isFilterMatched"
                                                       ng-click = "HomeSearchGrid.saveSummarySidePanel()"></i>-->
                                               </p> 
                                           </div>
   
   
                                           <div class="col-md-12 col-sm-12">
                                           <p class="col-md-12 P0 sectionText" translate="Summary_for_record_s" translate-values="{{ { number_of_records: HomeSearchGrid.TotalRecords } }}"></p>
                                           <table class="col-md-12 P0 summartTotal" ng-show = "HomeSearchGrid.summaryDisplayedColumns.length>0">
                                               <thead class="summary-totals-section-header">
                                                   <tr>
                                                       <th class="color727983 text-capitalize col-md-8 P0">{{ 'visible_columns' | translate }}</th>
                                                       <th class="color727983 text-capitalize col-md-4 P0">{{ 'Label_Total' | translate }}</th>
                                                   </tr>
                                               </thead>
                                               <tbody>
                                                   <tr class="color727983"  ng-repeat="totalRec in HomeSearchGrid.summaryDisplayedColumns track by $index" ng-if="totalRec.IsDisplayed && HomeSearchGrid.showExportDisplayColumns(totalRec.Label)">
                                                       <td class="text-capitalize">
                                                            <span ng-if="HomeSearchGrid.LabelChangeMap[totalRec.Label]">{{HomeSearchGrid.LabelChangeMap[totalRec.Label]}}</span>
                                                            <span ng-if="!HomeSearchGrid.LabelChangeMap[totalRec.Label]">{{totalRec.Label}}</span>
                                                       </td>
                                                       <td ng-if="totalRec.Type == 'Number'">{{totalRec.TotalValue | number:2}}</td>
                                                       <td ng-if="totalRec.Type == 'Currency'">{{totalRec.TotalValue | currency}}</td>
                                                   </tr>
                                               </tbody>
                                           </table>
                                           <div class="col-md-12 col-sm-12 P0 MT20" 
                                               ng-show = "HomeSearchGrid.dropdownSummaryDisplayedColumns.length != 0 || HomeSearchGrid.summaryHiddenColumns.length != 0">
                                               <div class = "savecancle_btncntnr summaryDropdownList">
                                                   <button class = "btn btn-default grayBtn col-md-12 col-sm-12 text-uppercase" data-toggle="dropdown" ng-click="HomeSearchGrid.seeMoreTotalsDropdown()" ng-disabled="HomeSearchGrid.disableTaxDetailReportSidebarButton">{{ 'see_more_totals' | translate }}</button>
                                              
                                               <ul class="add_info_dropdown dropdown-menu keep_open summaryDropDown" id = "summaryDropDown">
                                                   <div class="right-inner-addon" style="margin: 0 10px;">
                                                       <i class="glyphicon glyphicon-search"></i> 
                                                       <input class="form-control" ng-model="HomeSearchGrid.summaryMoreTotal.Label" 
                                                                placeholder="{{ 'Search' | translate }}" type="search" />
                                                   </div>
   
                                                   <li  ng-repeat="totalRec in HomeSearchGrid.dropdownSummaryDisplayedColumns | filter: HomeSearchGrid.summaryMoreTotal track by $index" ng-if="HomeSearchGrid.showExportDisplayColumns(totalRec.Label)" ng-click="totalRec.IsDisplayed = !totalRec.IsDisplayed" >
                                                       <p class="chk_tick" ng-attr-tabindex="100" ng-class="{'chked' : totalRec.IsDisplayed}">
                                                           <i class="fa fa-check-square chksqure"></i>
                                                       </p>
                                                       <span class = "seeMoreSummaryText T7">
                                                           <span ng-if="HomeSearchGrid.LabelChangeMap[totalRec.Label]">{{HomeSearchGrid.LabelChangeMap[totalRec.Label]}}</span>
                                                           <span ng-if="!HomeSearchGrid.LabelChangeMap[totalRec.Label]">{{totalRec.Label}}</span>
                                                       </span>
                                                   </li>
                                                   <li  ng-repeat="totalRec in HomeSearchGrid.summaryHiddenColumns | filter: HomeSearchGrid.summaryMoreTotal " ng-click="totalRec.IsDisplayed = !totalRec.IsDisplayed" >
                                                       <p class="chk_tick" ng-attr-tabindex="100" ng-class="{'chked' : totalRec.IsDisplayed}">
                                                           <i class="fa fa-check-square chksqure"></i>
                                                       </p>
                                                       <span class = "seeMoreSummaryText T7">
                                                           <span ng-if="HomeSearchGrid.LabelChangeMap[totalRec.Label]">{{HomeSearchGrid.LabelChangeMap[totalRec.Label]}}</span>
                                                           <span ng-if="!HomeSearchGrid.LabelChangeMap[totalRec.Label]">{{totalRec.Label}}</span>
                                                       </span>
                                                   </li>
                                               <div class="summaryHeader col-md-12 col-sm-12">
                                                   <div class="savecancle_btncntnr">
                                                       <button class="btn btn-default greenBtn text-uppercase" ng-click = "HomeSearchGrid.updateSummaryRecord()" > {{ 'Label_Apply' | translate }} </button>
                                                    </div>
                                               </div>
                                               </ul>
                                           </div>
                                           </div>
                                       </div>
                                       </div>
                                   
                                   
                                   
                                       <div class = "exportWrapper" ng-show = "HomeSearchGrid.exportDiv">
                                           <div  class="summaryHeader col-md-12 col-sm-12">
                                               <p class="col-md-12 col-sm-12 P0 BBW">
                                                   <i class="fa fa-print F25 color727983 pull-left summaryIcon"></i>
                                                   <span class="summaryHeadding T3 pull-left">{{ 'export_Print' | translate }}</span>
                                                   <i class="fa fa-times pull-right color727983 CP F20 PB5 closeSummary" aria-hidden="true" 
                                                       ng-click = "HomeSearchGrid.hideSidePanel()"></i>
                                                   <!--<i class="fa fa-cloud-upload pull-right color727983 CP F20 PB5 closeSummary" aria-hidden="true" 
                                                       ng-show="HomeSearchGrid.isFilterMatched"
                                                       ng-click = "HomeSearchGrid.saveSummarySidePanel()"></i>-->
                                                   
                                               </p>
                                           </div>
                                           <div class="col-md-12 col-sm-12">
                                           <p class="col-md-12 col-sm-12 P0 sectionText" translate="Export_for_record_s" translate-values="{{ { total_records: HomeSearchGrid.TotalRecords } }}"></p>
                                           <table class="col-md-12 col-sm-12 P0 summartTotal" 
                                                ng-show = "HomeSearchGrid.exportDisplayedColumns.length>0">
                                               <thead class="export-totals-section-header">
                                                   <tr>
                                                       <th class="color727983 text-capitalize col-md-8 col-sm-8 P0">{{ 'Totals_to_include' | translate }}:</th>
                                                       <th class="color727983 text-capitalize col-md-4 col-sm-4 P0">{{ 'Label_Total' | translate }}</th>
                                                   </tr>
                                               </thead>
                                               <tbody>
   
                                                   <tr class="color727983" ng-repeat="totalRec in HomeSearchGrid.exportDisplayedColumns track by $index" ng-if="totalRec.IsDisplayed && HomeSearchGrid.showExportDisplayColumns(totalRec.Label)">
                                                       <td class="text-capitalize">
                                                            <span ng-if="HomeSearchGrid.LabelChangeMap[totalRec.Label]">{{HomeSearchGrid.LabelChangeMap[totalRec.Label]}}</span>
                                                            <span ng-if="!HomeSearchGrid.LabelChangeMap[totalRec.Label]">{{totalRec.Label}}</span>
                                                       </td>
                                                       <td ng-if="totalRec.Type == 'Number'">{{totalRec.TotalValue | number:2}}</td>
                                                       <td ng-if="totalRec.Type == 'Currency'">{{totalRec.TotalValue | currency}}</td>
                                                   </tr>
                                               </tbody>
                                           </table>
                                           <div class="col-md-12 col-sm-12 P0"
                                               ng-show = "HomeSearchGrid.dropdownExportDisplayedColumns.length != 0 || HomeSearchGrid.exportHiddenColumns != 0">
                                               <div class = "savecancle_btncntnr exportDropdownList">
                                                   <button class = "btn btn-default grayBtn col-md-12 col-sm-12 text-uppercase uppercase" data-toggle="dropdown"  ng-click="HomeSearchGrid.ExportTotalsDropdown()" ng-disabled="HomeSearchGrid.disableTaxDetailReportSidebarButton">{{ 'select_totals_to_include' | translate }}</button>
                                               <ul class="add_info_dropdown dropdown-menu keep_open summaryDropDown" id="exportDropDown">
                                                   <div class="right-inner-addon" style="margin: 0 10px;">
                                                       <i class="glyphicon glyphicon-search"></i> 
                                                       <input class="form-control" ng-model="HomeSearchGrid.summaryMoreTotal.Label" 
                                                                placeholder="{{ 'Search' | translate }}" type="search" />
                                                   </div>
                                                   
                                                   <li  ng-repeat="totalRec in HomeSearchGrid.dropdownExportDisplayedColumns | filter: HomeSearchGrid.summaryMoreTotal track by $index" 
                                                           ng-if="HomeSearchGrid.showExportDisplayColumns(totalRec.Label)" ng-click="totalRec.IsDisplayed = !totalRec.IsDisplayed"  >
                                                       <p class="chk_tick" ng-attr-tabindex="100" ng-class="{'chked' : totalRec.IsDisplayed}">
                                                        <i class="fa fa-check-square chksqure"></i>
   
                                                      </p>
                                                       <span class = "seeMoreSummaryText T7">
                                                           <span ng-if="HomeSearchGrid.LabelChangeMap[totalRec.Label]">{{HomeSearchGrid.LabelChangeMap[totalRec.Label]}}</span>
                                                           <span ng-if="!HomeSearchGrid.LabelChangeMap[totalRec.Label]">{{totalRec.Label}}</span>
                                                       </span>
   
                                                   </li>
                                                   <li  ng-repeat="totalRec in HomeSearchGrid.exportHiddenColumns | filter: HomeSearchGrid.summaryMoreTotal " 
                                                           ng-if="HomeSearchGrid.showExportDisplayColumns(totalRec.Label)" ng-click="totalRec.IsDisplayed = !totalRec.IsDisplayed"  >
                                                       <p class="chk_tick" ng-attr-tabindex="100" ng-class="{'chked' : totalRec.IsDisplayed}">
                                                        <i class="fa fa-check-square chksqure"></i>
                                                      </p>
                                                       <span class = "seeMoreSummaryText T7">
                                                           <span ng-if="HomeSearchGrid.LabelChangeMap[totalRec.Label]">{{HomeSearchGrid.LabelChangeMap[totalRec.Label]}}</span>
                                                           <span ng-if="!HomeSearchGrid.LabelChangeMap[totalRec.Label]">{{totalRec.Label}}</span>
                                                       </span>
                                                   </li>
                                               <div class="summaryHeader col-md-12 col-sm-12">
                                                   <div class="savecancle_btncntnr">
                                                       <button class="btn btn-default greenBtn text-uppercase" ng-click="HomeSearchGrid.updateExportRecord()"> {{ 'Label_Apply' | translate }} </button>
                                                    </div>
                                               </div>
                                                   
                                           </ul>
                                       </div>

                                           </div>
                                       </div>
                                           <div class="summaryHeader col-md-12 col-sm-12">
                                               <p class="col-md-12 col-sm-12 P0 BBW"></p>
                                               <table class="col-md-12 col-sm-12 P0 summartTotal">
                                               <thead>
                                                   <tr>
                                                       <th class="color727983 text-capitalize col-md-8 col-sm-8 P0">{{ 'Group_Records_By' | translate }}:</th>
                                                   </tr>
                                               </thead>
                                               <tbody>
                                                   <tr class="color727983" >
                                                       <td class="text-capitalize">{{HomeSearchGrid.groupNameResult}}  <span ng-show="HomeSearchGrid.ClearGroupIcon()" class="fa fa-times CP"  ng-click="HomeSearchGrid.ClearGroup()" style="color:#727983"></span> </td>
                                                       
                                                   </tr>
                                               </tbody>
                                           </table>
                                           <div class="col-md-12 col-sm-12 MT20 P0 ">
                                               <div class="savecancle_btncntnr customDropdownDiv">
                                                   <button class = "btn btn-default grayBtn col-md-12 col-sm-12 text-uppercase" data-toggle="dropdown" ng-disabled="HomeSearchGrid.disableTaxDetailReportSidebarButton">{{ 'select_column_to_group' | translate }}</button>
                                               
                                               <ul class="add_info_dropdown dropdown-menu keep_open summaryDropDown">
                                                   <div class="right-inner-addon" style="margin: 0 10px;">
                                                       <i class="glyphicon glyphicon-search"></i> 
                                                       <input class="form-control" ng-model="HomeSearchGrid.summaryMoreTotal.Label" 
                                                                placeholder="{{ 'Search' | translate }}" type="search" />
                                                   </div>
                                                   
                                                   <li  ng-repeat="totalRec in HomeSearchGrid.GridSummaryDataForExport | filter : HomeSearchGrid.summaryMoreTotal "
                                                   ng-class="{'activeFilter' : HomeSearchGrid.groupNameResult == totalRec.Label}" ng-if="((((HomeSearchGrid.showFieldToGroup(totalRec.Type, totalRec.Label) || 
                                                                           (totalRec.Type == 'Text' || (totalRec.Type == 'Link' && (totalRec.Label == 'Item' || totalRec.Label == 'Invoice Number' ||  
                                                                           totalRec.Label == 'VO Number' || (totalRec.Label == 'Needed For' && HomeSearchGrid.isObjectSelected('VO Items')))) && 
                                                                           (totalRec.Label != 'Active' || HomeSearchGrid.IsIncludeInactiveRecords))) || 
                                                                           totalRec.Type == 'Date') && !HomeSearchGrid.isObjectSelected('Claim Jobs')) || (HomeSearchGrid.isVisibleClaimJobsExportColumn(totalRec.Label) && HomeSearchGrid.isObjectSelected('Claim Jobs')))" ng-click = "HomeSearchGrid.GroupExport(totalRec.Label, totalRec.RecordKey)">
                                                       <span class = "seeMoreSummaryText T7 PL10">{{totalRec.Label}}</span>
                                                       
                                                   </li>
                                           </ul>
                                       </div>
                                           </div>
                                           </div>
                                       
                                           <div class="summaryHeader col-md-12 col-sm-12">
                                               <p class="col-md-12 col-sm-12 P0 BBW"></p>
                                               <table class="col-md-12 col-sm-12 P0 summartTotal">
                                               <thead> 
                                                   <tr>
                                                       <th class="color727983 text-capitalize col-md-8 P0">{{ 'output_results_to' | translate }}:</th>
                                                   </tr>
                                               </thead>
                                               <tbody>
                                                   <tr class="color727983" >
                                                       <td class="text-capitalize">{{HomeSearchGrid.outputResult}}</td>
                                                   </tr>
                                               </tbody>
                                           </table>
                                           <div class="col-md-12 col-sm-12 MT20 P0 customDropdownDiv">
                                               <button class = "btn btn-default grayBtn col-md-12 col-sm-12 text-uppercase" data-toggle="dropdown" >{{ 'select_output_results' | translate }}</button>
                                               <ul class="add_info_dropdown dropdown-menu keep_open summaryDropDown">
                                                   <li ng-click = "HomeSearchGrid.outputExport('Excel (.csv)')" 
                                                   ng-class="{'activeFilter' : HomeSearchGrid.outputResult == 'Excel (.csv)'}">
                                                   <span class = "seeMoreSummaryText T7 PL10">{{ 'Excel_csv' | translate }}</span>
                                                   </li>
                                                   <li ng-click = "HomeSearchGrid.outputExport('PDF File')"
                                                   ng-class="{'activeFilter' : HomeSearchGrid.outputResult == 'PDF File'}" ><span class = "seeMoreSummaryText T7 PL10">{{ 'PDF_File' | translate }}</span>
                                                   </li>
                                                   <li ng-click = "HomeSearchGrid.outputExport('Printer')" 
                                                   ng-class="{'activeFilter' : HomeSearchGrid.outputResult == 'Printer'}"><span class = "seeMoreSummaryText T7 PL10">{{ 'Printer' | translate }}</span>
                                                   </li>       
                                           </ul>
                                           
                                           </div>
                                           </div>
                                       
                                       
                                       
                                       <div class="summaryHeader col-md-12 col-sm-12 MB60 previewApplyBtnSection">
                                               <p class="col-md-12 col-sm-12 P0 BBW"></p>
                                           <div class="savecancle_btncntnr">
                                               <button class="btn btn-default grayBtn text-uppercase" ng-click="HomeSearchGrid.PreviewGridRecordsAction('Preview')"> {{ 'Preview' | translate }} </button>
                                               <button class="btn btn-default greenBtn text-uppercase" ng-if = "HomeSearchGrid.outputResult != '' && HomeSearchGrid.outputResult != undefined" ng-click="HomeSearchGrid.PreviewGridRecordsAction('Export')"> {{ 'Label_Apply' | translate }} </button>
                                               <button class="btn btn-default grayBtn text-uppercase" ng-if = "HomeSearchGrid.outputResult == '' || HomeSearchGrid.outputResult == undefined"> {{ 'Label_Apply' | translate }} </button>
                                            </div>
                                      </div>
                                       
                                       </div>
                                   </div>
                                   </div>    
                            
                                   <!-- Side Panel Div end here -->
                            
                                 <!--  Export/Print Div start here -->
                                 
                                 
                                <!--  Export/Print Div end here -->
                            
                            </div>
                            <!-- filter Div End here --> 
                            <div class="row M0 gid_container_wrapper ">
                           <div class="gid_container_Customer pull-left">
                               <div class="resultDiv" ng-if="HomeSearchGrid.SearchedResult.length != 0">
                                   <i class="fa fa-caret-down" id="dropArrowIcon"></i>
                                   <div class = "pull-right" style="width: 6%; margin-top: 6px;" id = "HomeSearchresultDiv">
                                   <div style="padding: 0;background: #fff;"> 
                                                   <div class="customDropdown" ng-class="{'colDynamicWidth': HomeSearchGrid.isSidePanelToggle} ">       
                                                       <div class="dropdown">
                                                            <button type="button" data-toggle="dropdown"
                                                               class="my-dropdown btn btn-default dropdown-toggle"
                                                               ng-click="HomeSearchGrid.openColumnsDropdown()">
                                                               <span class="filter-wrap"> 
                                                                   <span class="filterLabel" ng-show="!HomeSearchGrid.loadingColumn"><i class="fa fa-plus"></i></span>
                                                               </span>   
                                                               <span class="caret"></span>
                                                           </button>
                                                            <ul class="column_dropdown chkbox_dropdown dropdown-menu keep_open " ng-class="{'dropDownAfterSummary': HomeSearchGrid.isSidePanelToggle} "  id="column_dropdown">
                                                                   <h4 class="columnHeading">{{ 'Columns' | translate }}</h4>
                                                                    <div class="right-inner-addon">
                                                                      <i class="glyphicon glyphicon-search"></i> <input type="search"
                                                                          class="form-control" placeholder="{{ 'Search' | translate }}"
                                                                          ng-model="HomeSearchGrid.columnSearch.Label" />
                                                                  </div>
                                                                  <hr />
                                                                  <a class="restoreDefaultTxt" ng-click="HomeSearchGrid.restoreColumns()">{{ 'Restore_Defaults' | translate }}</a>
                                                                  <div class="column_fixHeight" id="additional_column_fix">
                                                                      <li ng-repeat="displayedCol in HomeSearchGrid.columns.DisplayedColumns | filter:HomeSearchGrid.columnSearch"
                                                                           ng-click="displayedCol.IsDisplayed = !displayedCol.IsDisplayed" ng-if="HomeSearchGrid.showColumn(displayedCol.Label);"> 
                                                                          <label>
                                                                            <p class="chk_tick" ng-class="{'chked': displayedCol.IsDisplayed}">
                                                                               <i class="fa fa-check-square chksqure"></i>
                                                                            </p>
                                                                            <label class="css-label"></label>
                                                                               <span ng-if="HomeSearchGrid.LabelChangeMap[displayedCol.Label]">{{HomeSearchGrid.LabelChangeMap[displayedCol.Label]}}</span>
                                                                               <span ng-if="!HomeSearchGrid.LabelChangeMap[displayedCol.Label] && displayedCol.Label.toString().toLowerCase() !== 'model' && displayedCol.Label.toString().toLowerCase() !== 'submodel'">{{displayedCol.Label}}</span>
                                                                               <span ng-if="!HomeSearchGrid.LabelChangeMap[displayedCol.Label] && displayedCol.Label.toString().toLowerCase() == 'model' ">{{ Custom_Label_Model | translate }}</span>
                                                                               <span ng-if="!HomeSearchGrid.LabelChangeMap[displayedCol.Label] && displayedCol.Label.toString().toLowerCase() == 'submodel'">{{ Custom_Label_Sub_Model | translate }}</span>
                                                                        </label>
                                                                      </li>
                                                                      <li ng-repeat="hiddenCol in HomeSearchGrid.columns.HiddenColumns  | filter:HomeSearchGrid.columnSearch"
