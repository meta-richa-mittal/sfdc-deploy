                                                            {{ col.RecordKey.includes('Taxable:') ? data['TaxItemRecs'][col.Label.split(":").pop()][0] : data['TaxItemRecs'][col.Label.split(":").pop()][1]  | currency }}
                                                       </span>
                                                       
                                                       <span ng-if="col.Type == 'Text' || col.Type == 'Email'  || col.Type == 'Date'">
                                                            {{data[col.RecordKey]}}
                                                       </span>
                                                       <span ng-if="col.Type == 'Phone'">
                                                            {{data[col.RecordKey] }}
                                                       </span>
                                                       <span ng-if="col.Type == 'Boolean'">
                                                            {{data[col.RecordKey] ? 'Yes' : 'No'}}
                                                       </span>
                                                  </td>
                                               </tr> 
                                           </tbody> 
                                       </table>
                                   </div>
                                   <!-- Pagination container -->
                                   <div class="row M0 pagination_container" id="HSGrid_Pagination" ng-if="HomeSearchGrid.SearchedResult.length != 0 && 
