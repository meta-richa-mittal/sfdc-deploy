                                                                <a id="VV_Product_block_grid_container_tbody_expandtr_edit_box_its-heading_edit-body_{{$index}}_go_btn" 
                                                                    class="btn btn-default editform_btn" role="button"
                                                                    ng-click="ViewVendorRelatedListModal.Products_GoAction($index)">{!$Label.Label_Apply}</a>                                
                                                            </form>
                                                        </div>
                                                    </div>
                                                </td>
                                                <td style="background:#fff"></td>
                                            </tr>
                                            <!-- END: Actions row in case editable row -->
                                        </tbody>
                                    </table>
