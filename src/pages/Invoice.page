                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!isHidePartNumbersOnDocuments && isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                <col width="50%" />
                                        </apex:outputPanel>

