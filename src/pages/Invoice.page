                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                <col width="10%" />
                                        </apex:outputPanel> 
                                        <col width="10%" /> 
                                        <col width="10%" /> 
