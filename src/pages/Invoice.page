                                                <th class="c-text-align-right">Qty</th>
                                                <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                    <th class="c-text-align-right">MSRP
                                                        </th>
                                                </apex:outputPanel>
                                                <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                    <th class="c-text-align-right">Disc
                                                    </th>
                                                </apex:outputPanel>
                                                <th class="c-text-align-right">Price</th>
                                                <th class="c-text-align-right">Total</th>
                                            </tr>
                                        </thead>
                                        <apex:repeat value="{!CustomerOrderInvoiceDetailRecord.COKHList}" var="COKHItem">
                                            <tbody>
                                                <apex:outputText rendered="{!COKHItem.Id != null}">
                                                    <tr> 
                                                        <apex:outputPanel rendered="{!!isHidePartNumbersOnDocuments}" layout="none">
                                                            <td class="c-text-uppercase">
                                                                Kit
                                                            </td>
                                                        </apex:outputPanel>
                                                        <td>{!COKHItem.ItemDescription}</td>
                                                        <td></td>
                                                        <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">    
                                                        <td></td>
                                                        </apex:outputPanel>
                                                        <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">

                                                            <td></td>
                                                        </apex:outputPanel>
                                                        <td></td>
                                                        <td class="c-text-align-right">
                                                            <apex:outputText value="{0, number, currency}" >
                                                                <apex:param value="{!COKHItem.Price}"/>
                                                            </apex:outputText>
                                                        </td>
                                                    </tr>
                                                </apex:outputText>
                                                <apex:repeat value="{!COKHItem.COLIList}" var="COlitem" rendered="{!((COKHItem.Id == null) || (COKHItem.Id != null && !isHideKitDetailsOnDocuments))}">
                                                <tr >
                                                        <apex:outputPanel rendered="{!!isHidePartNumbersOnDocuments}" layout="none">
                                                        <td class="c-text-uppercase">
                                                            <apex:outputText rendered="{!COKHItem.Id == null}" value="{!COlitem.ItemCode}"></apex:outputText>
                                                        </td> 
                                                        </apex:outputPanel>
                                                        <td >
                                                            <apex:outputText rendered="{!((COKHItem.Id == null || (isHidePartNumbersOnDocuments && COlitem.PartId != null)) && CustomerOrderInvoiceDetailRecord.COKHList[0].COLIList[0].DealId == null || (CustomerOrderInvoiceDetailRecord.COKHList[0].COLIList[0].DealId != null && !isFinalized))}" value="{!COlitem.ItemDescription}"></apex:outputText>
                                                            <apex:outputText rendered="{!(COKHItem.Id != null && (COlitem.PartId == null || !isHidePartNumbersOnDocuments) && CustomerOrderInvoiceDetailRecord.COKHList[0].COLIList[0].DealId == null || (CustomerOrderInvoiceDetailRecord.COKHList[0].COLIList[0].DealId != null && !isFinalized))}" >
                                                                <apex:outputText styleClass="c-text-uppercase" value="{!COlitem.ItemCode}"></apex:outputText> - <apex:outputText value=" {!COlitem.ItemDescription}"></apex:outputText>
                                                            </apex:outputText>
                                                        
                                                        </td>
                                                        <td class="c-text-align-right">
                                                            <apex:outputText value="{0}">
                                                                <apex:param value="{!if(COKHItem.Id == null && COlitem.IsPart && !COlitem.NonInventoryPart && COlitem.Qty > 0 && CustomerOrderInvoiceDetailRecord.OrderStatus != 'Quote', COlitem.QtyCommitted, COlitem.Qty)}" />
                                                            </apex:outputText>
                                                        </td>
                                                        <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                            <td class="c-text-align-right"> 
                                                                <apex:outputText value="{0, number, currency}" rendered="{!COlitem.IsPart}">
                                                                    <apex:param value="{!COlitem.CalculatedMSRP}"/>
                                                                </apex:outputText>
                                                            </td>
                                                    </apex:outputPanel>
                                                    <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                        <td class="c-text-align-right"> 
                                                            <apex:outputText value="{0, number, currency}" rendered="{!COlitem.IsPart}">
                                                                <apex:param value="{!COlitem.DiscountAmount}"/>
                                                            </apex:outputText>
                                                        </td>
                                                </apex:outputPanel>
                                                    
                                                        <td class="c-text-align-right">
                                                            <apex:outputText value="{0, number, currency}" >
                                                                <apex:param value="{!COlitem.Price}"/>
                                                            </apex:outputText>
                                                        </td>
                                                        <td class="c-text-align-right">
                                                            <apex:outputPanel rendered="{!COKHItem.Id == null}" layout="none">
                                                                <apex:outputText value="{0, number, currency}" rendered="{!COlitem.SubTotal == null}">
                                                                    <apex:param value="{!if(COKHItem.Id == null && COlitem.IsPart && !COlitem.NonInventoryPart && COlitem.Qty > 0 && CustomerOrderInvoiceDetailRecord.OrderStatus != 'Quote', COlitem.QtyCommitted * COlitem.Price, COlitem.Qty * COlitem.Price)}"/>
                                                                </apex:outputText>
                                                                <apex:outputText value="{0, number, currency}" rendered="{!COlitem.SubTotal != null}">
                                                                    <apex:param value="{!if(COKHItem.Id == null && COlitem.IsPart && !COlitem.NonInventoryPart && COlitem.Qty > 0 && CustomerOrderInvoiceDetailRecord.OrderStatus != 'Quote', COlitem.QtyCommitted * COlitem.Price, COlitem.SubTotal)}"/>
                                                                </apex:outputText>
                                                            </apex:outputPanel>
                                                        </td>
                                                    </tr>
                                                </apex:repeat>
                                            </tbody>
                                        </apex:repeat>
                                    </table>
                                </apex:outputPanel>
                                </td>
                            </tr>
