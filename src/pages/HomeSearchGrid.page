<apex:page standardStylesheets="false" showChat="false" showHeader="false" sidebar="false" docType="html-5.0"
    controller="GlobalController">
      <route-loading-indicator></route-loading-indicator>
      <div ng-cloak="ng-cloak"> <!-- data-ng-controller="searchTableController" data-ng-init="HomeSearchGrid.LoadGrid()" ng-cloak="ng-cloak" commented by kajal 9 Mar. 2017 -->
         <!-- content start here -->
        <div class="Container-fluid" style="margin-top: 48px;">
            <div class="row M0"> 
                   <!-- Side panel start -->
                  <div class="col-lg-2 col-md-4 col-sm-4 P0 sidepanel">  
                        <div class="navbar-default row M0 P0">
                            <div class="sidebarSearchbox col-md-12 col-sm-12 P0">
                                <i class="fa fa-search fa-flip-horizontal"></i>
                                <input type="search" class="sideBarSearch" placeholder="{{ 'Sidebar_search' | translate }}" ng-model="HomeSearchGrid.sidebarSearch.FilterLabel"
                                    ng-keyup = "HomeSearchGrid.collapseAllFilter()"/>
                            </div>  
                            <div class="P0 col-md-12 col-sm-12 MB2" >
                                <div class="filterHeaddingWrapper P0 col-sm-12 col-md-12" ng-click="HomeSearchGrid.isfavorite= !HomeSearchGrid.isfavorite" >
                                    <span class="col-md-3 col-sm-3"> 
                                        <i class="fa fa-angle-down downArrow" ng-show="HomeSearchGrid.isfavorite"></i>
                                        <i class="fa fa-angle-right downArrow" ng-show="!HomeSearchGrid.isfavorite"></i>
                                    </span>
                                    <p class="filterHeadding text-uppercase T3 col-md-8 col-sm-8 P0">{{ 'favorite_search' | translate }}</p>
                                </div>
                                <div class="homeSearchFilterListwrapper P0 col-md-12 col-sm-12 filterWrapper" ng-show="HomeSearchGrid.isfavorite"  >
                                    <ul class="homeSearchFilterList P0 col-md-12 col-sm-12" >
                                        <li class="P0 col-md-12  col-sm-12"  ng-mouseover="show = true" ng-mouseleave="show = false" 
                                            ng-repeat="favoriteFilter  in HomeSearchGrid.filterList.FilterType.Favorite | orderBy:'FilterLabel' | filter:HomeSearchGrid.sidebarSearch"
                                            ng-class="{'activeFilter' : HomeSearchGrid.isfilterActive(favoriteFilter.Id, 'favorite')}">
                                            <span class="col-md-3 col-sm-3 pull-left"> <i class="fa fa-star pull-left"></i></span>
                                            <i class="bp-icons bp-font-14 bp-blue-font report-scheduling-cal-icon-custom" ng-if="favoriteFilter.ScheduleReportObj.IsReportScheduled" 
                                                title="{{HomeSearchGrid.getScheduledReportTooltip(favoriteFilter)}}" data-toggle="tooltip"
                                                ng-class="{'bp-white-font': HomeSearchGrid.isfilterActive(favoriteFilter.Id, 'favorite')}">calendarFilled</i>
                                            <p class="text-uppercase T3 col-sm-7 col-md-7 pull-left P0" data-original-title="{{favoriteFilter.FilterLabel}}" data-toggle="tooltip" ng-click="HomeSearchGrid.applySavedFilter(favoriteFilter, 'favorite' )">
                                                {{favoriteFilter.FilterLabel}}
                                            </p>
                                            <span class="col-md-1 col-sm-1 P0 dropdown-toggle pull-left" data-toggle="dropdown" role="button"  ng-show="show"><i class="fa fa-cog PT5 "></i></span>
                                             <ul class="dropdown-menu" role="menu">
                                                <li>
                                                    <a class="T7" ng-class="{'bp-disabled': favoriteFilter.ScheduleReportObj.showLoading}" ng-if="HomeSearchGrid.reportScheduling.isReportSchedulingActionAvailable() && favoriteFilter.ScheduleReportObj.IsReportSchedulable" ng-click="HomeSearchGrid.scheduleReport(favoriteFilter)">{{(favoriteFilter.ScheduleReportObj.IsReportScheduled ? 'Edit_report_scheduling' : 'Report_scheduling') | translate }}</a>
                                                    <i class="spinner-loading-icon" ng-if = "favoriteFilter.ScheduleReportObj.showLoading">
                                                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="200px" height="200px" viewBox="0 0 100 100" preserveAspectRatio="xMidYMid">
                                                            <circle cx="50" cy="50" fill="none" stroke="#3384FF" stroke-width="10" r="35" stroke-dasharray="164.93361431346415 56.97787143782138">
                                                                <animateTransform attributeName="transform" type="rotate" repeatCount="indefinite" dur="1s" values="0 50 50;360 50 50" keyTimes="0;1"></animateTransform>
                                                            </circle>
                                                        </svg>
                                                    </i>
                                                </li>
                                                <li ng-click="HomeSearchGrid.deleteScheduledReport(favoriteFilter.Id, 'Favorite')" ng-if="HomeSearchGrid.reportScheduling.isReportSchedulingActionAvailable() && favoriteFilter.ScheduleReportObj.IsReportScheduled">
                                                    <a  class="T7" >{{ 'Delete_report_scheduling' | translate }}</a>
                                                </li>
                                                <li ng-click="HomeSearchGrid.markFilterFav(favoriteFilter.Id, false)">
                                                    <a  class="T7" >{{ 'Remove_From_Favorites' | translate }}</a>
                                                </li>
                                                <li ng-click="HomeSearchGrid.renameOrCopyCustomFilter(favoriteFilter.Id, favoriteFilter.FilterLabel, false)"
                                                    ng-hide="favoriteFilter.IsCommon">
                                                    <a class="T7">{{ 'Rename' | translate }}</a>
                                                </li>
                                                <li ng-click="HomeSearchGrid.renameOrCopyCustomFilter(favoriteFilter.Id, favoriteFilter.FilterLabel, true)">
                                                    <a class="T7">{{ 'Copy' | translate }}</a>
                                                </li>
                                                <li ng-click="HomeSearchGrid.confirmDelete(favoriteFilter.Id, favoriteFilter.FilterLabel)"
                                                    ng-hide="favoriteFilter.IsCommon">
                                                    <a class="T7">{{ 'Delete_Label' | translate }}</a>
                                                </li>
                                                
                                            </ul>
                                        </li>
                                    </ul>
                            </div>
                            </div>
                            <div class="P0 col-md-12 col-sm-12 MB2">
                                <div class="filterHeaddingWrapper P0 col-md-12 col-sm-12" ng-click="HomeSearchGrid.iscommon= !HomeSearchGrid.iscommon">
                                    <span class="col-md-3 col-sm-3"> 
                                        <i class="fa fa-angle-down downArrow" ng-show="HomeSearchGrid.iscommon"></i>
                                        <i class="fa fa-angle-right downArrow" ng-show="!HomeSearchGrid.iscommon"></i>
                                    </span>
                                    <p class="filterHeadding text-uppercase T3 col-md-8 col-sm-8 P0">{{ 'COMMON_SEARCH' | translate }}</p>
                                </div>
                                
                                <div class="col-md-12 col-sm-12 P0 filterWrapper " ng-repeat="commonFilter  in HomeSearchGrid.filterList.FilterType.Common | orderBy : 'Order'" ng-show="HomeSearchGrid.iscommon">
                                    <div class="filterHeadding P0 col-md-12  ">
                                        <span class="col-md-3 col-sm-3"  ng-if="commonFilter.FilterLabel=='Customer Sales'"> <i class="fa fa-male"></i></span>
                                        <span class="col-md-3 col-sm-3" ng-if="commonFilter.FilterLabel=='Product'"> <i class="fa fa-suitcase" ></i></span>
                                        <span class="col-md-3 col-sm-3" ng-if="commonFilter.FilterLabel=='People'"> <i class="fa fa-users" ></i></span>
                                        <span class="col-md-3 col-sm-3" ng-if="commonFilter.FilterLabel=='Settings'"> <i class="fa fa-wrench" ></i></span>
                                        <span class="col-md-3 col-sm-3" ng-if="commonFilter.FilterLabel=='Vendor Orders'"> <i class="fa fa-shopping-cart" ></i></span>
                                            <!-- START: Added by Dinesh Kumar - #1154 - 10/06/2016 -->
                                            <span class="col-md-3 col-sm-3" ng-if="commonFilter.FilterLabel=='Reporting'"> <i class="fa fa-times" ></i></span>
                                            <!-- END: Added by Dinesh Kumar - #1154 - 10/06/2016 -->
    
    
                                        <p class="filterHeadding MT9  text-uppercase T3 col-md-8 col-sm-8 P0 commonHeadding">{{commonFilter.FilterLabel}}</p>
    
                                    </div>
                                    <div class="homeSearchFilterListwrapper commonSearchFilterWrapper P0 col-md-12 col-sm-12">
                                        <ul class="homeSearchFilterList P0 col-md-12 col-sm-12 commonSearchFilter">
                                            <li class=" col-md-12 col-sm-12" ng-repeat="commonfilterval in commonFilter.subgroupList | orderBy : 'Order' | filter:HomeSearchGrid.sidebarSearch" 
                                                ng-mouseover="show = true" ng-mouseleave="show = false"
                                                ng-class="{'activeFilter' : HomeSearchGrid.isfilterActive(commonfilterval.Id, 'Common')}">
                                                <i class="bp-icons bp-font-14 bp-blue-font report-scheduling-cal-icon-common" ng-if="commonfilterval.ScheduleReportObj.IsReportScheduled" 
                                                    title="{{HomeSearchGrid.getScheduledReportTooltip(commonfilterval)}}" data-toggle="tooltip"
                                                    ng-class="{'bp-white-font': HomeSearchGrid.isfilterActive(commonfilterval.Id, 'Common')}">calendarFilled</i>
                                                <p class="text-uppercase FT col-md-7 col-sm-11 P0 col-md-offset-3" 
                                                ng-click="HomeSearchGrid.applySavedFilter(commonfilterval, 'Common')"><!-- data-original-title="{{commonfilterval.FilterLabel}}" data-toggle="tooltip" removed 7th march, 2017 by kajal #1587 -->
                                                    {{commonfilterval.FilterLabel}}
                                                </p>
                                                <span class="col-md-1 col-sm-1 P0 dropdown-toggle" data-toggle="dropdown" role="button" ng-show="show"><i class="fa fa-cog "></i></span>
                                                 <ul class="dropdown-menu" role="menu">
                                                    <li>
                                                        <a class="T7" ng-class="{'bp-disabled': commonfilterval.ScheduleReportObj.showLoading}" ng-if="HomeSearchGrid.reportScheduling.isReportSchedulingActionAvailable() && commonfilterval.ScheduleReportObj.IsReportSchedulable" ng-click="HomeSearchGrid.scheduleReport(commonfilterval)">{{(commonfilterval.ScheduleReportObj.IsReportScheduled ? 'Edit_report_scheduling' : 'Report_scheduling') | translate }}</a>
                                                        <i class="spinner-loading-icon" ng-if = "commonfilterval.ScheduleReportObj.showLoading">
                                                            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="200px" height="200px" viewBox="0 0 100 100" preserveAspectRatio="xMidYMid">
                                                                <circle cx="50" cy="50" fill="none" stroke="#3384FF" stroke-width="10" r="35" stroke-dasharray="164.93361431346415 56.97787143782138">
                                                                    <animateTransform attributeName="transform" type="rotate" repeatCount="indefinite" dur="1s" values="0 50 50;360 50 50" keyTimes="0;1"></animateTransform>
                                                                </circle>
                                                            </svg>
                                                        </i>
                                                    </li>
                                                    <li ng-click="HomeSearchGrid.deleteScheduledReport(commonfilterval.Id, 'Common', commonFilter.FilterLabel)" ng-if="HomeSearchGrid.reportScheduling.isReportSchedulingActionAvailable() && commonfilterval.ScheduleReportObj.IsReportScheduled">
                                                        <a  class="T7" >{{ 'Delete_report_scheduling' | translate }}</a>
                                                    </li>
                                                    <li><a class="T7" ng-click="HomeSearchGrid.renameOrCopyCustomFilter(commonfilterval.Id, commonfilterval.FilterLabel, true)">{{ 'Copy' | translate }}</a></li>
                                                </ul>
                                            </li>
                                        </ul>
                                    </div>
                            </div>
                            </div>

                            <div class="P0 col-md-12 col-sm-12 MB2" ng-if="GroupOnlyPermissions['Accounting Integration']['enabled']">
                                <div class="filterHeaddingWrapper P0 col-md-12 col-sm-12" ng-click="HomeSearchGrid.isReconciliation= !HomeSearchGrid.isReconciliation">
                                    <span class="col-md-3 col-sm-3"> 
                                        <i class="fa fa-angle-down downArrow" ng-show = "HomeSearchGrid.isReconciliation"></i>
                                        <i class="fa fa-angle-right downArrow" ng-show = "!HomeSearchGrid.isReconciliation"></i>
                                    </span>
                                    <p class="filterHeadding text-uppercase T3 col-md-8 col-sm-8 P0">Reconciliations</p>
                                </div>
                                <div class="homeSearchFilterListwrapper P0 col-md-12 col-sm-12 filterWrapper" ng-show = "HomeSearchGrid.isReconciliation">
                                    <ul class="homeSearchFilterList P0 col-md-12 col-sm-12 commonSearchFilter">
                                        <li class="P0 col-md-12 col-sm-12 reconciliation" ng-repeat="reconciliationFilter  in HomeSearchGrid.filterList.FilterType.Reconciliation" 
                                            ng-mouseover="show = true" ng-mouseleave="show = false"
                                            ng-class="{'activeFilter' : HomeSearchGrid.isfilterActive(reconciliationFilter.Id, 'Reconciliation')}">
                                            <span class="col-md-3 col-sm-3 text-center"></span>
                                            <i class="bp-icons bp-font-14 bp-blue-font report-scheduling-cal-icon-reconciliation" ng-if="reconciliationFilter.ScheduleReportObj.IsReportScheduled" 
                                                title="{{HomeSearchGrid.getScheduledReportTooltip(reconciliationFilter)}}" data-toggle="tooltip"    
                                                ng-class="{'bp-white-font': HomeSearchGrid.isfilterActive(reconciliationFilter.Id, 'Reconciliation')}">calendarFilled</i>
                                            <p class="text-uppercase T3 col-md-7 col-sm-7 P0"  
                                                ng-click="HomeSearchGrid.applySavedFilter(reconciliationFilter, 'Reconciliation')">
                                                {{reconciliationFilter.FilterLabel}}
                                            </p>
                                            <span class="col-md-1 col-sm-1 P0 dropdown-toggle" data-toggle="dropdown" role="button" ng-show="show"><i class="fa fa-cog "></i></span>
                                            <ul class="dropdown-menu" role="menu">
                                                <li>
                                                    <a class="T7" ng-class="{'bp-disabled': reconciliationFilter.ScheduleReportObj.showLoading}" ng-if="HomeSearchGrid.reportScheduling.isReportSchedulingActionAvailable() && reconciliationFilter.ScheduleReportObj.IsReportSchedulable" ng-click="HomeSearchGrid.scheduleReport(reconciliationFilter)">{{(reconciliationFilter.ScheduleReportObj.IsReportScheduled ? 'Edit_report_scheduling' : 'Report_scheduling') | translate }}</a>
                                                    <i class="spinner-loading-icon" ng-if = "reconciliationFilter.ScheduleReportObj.showLoading">
                                                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="200px" height="200px" viewBox="0 0 100 100" preserveAspectRatio="xMidYMid">
                                                            <circle cx="50" cy="50" fill="none" stroke="#3384FF" stroke-width="10" r="35" stroke-dasharray="164.93361431346415 56.97787143782138">
                                                                <animateTransform attributeName="transform" type="rotate" repeatCount="indefinite" dur="1s" values="0 50 50;360 50 50" keyTimes="0;1"></animateTransform>
                                                            </circle>
                                                        </svg>
                                                    </i>
                                                </li>
                                                <li ng-click="HomeSearchGrid.deleteScheduledReport(reconciliationFilter.Id, 'Reconciliation', reconciliationFilter.FilterLabel)" ng-if="HomeSearchGrid.reportScheduling.isReportSchedulingActionAvailable() && reconciliationFilter.ScheduleReportObj.IsReportScheduled">
                                                    <a  class="T7" >{{ 'Delete_report_scheduling' | translate }}</a>
                                                </li>
                                                <li><a class="T7" ng-click="HomeSearchGrid.renameOrCopyCustomFilter(reconciliationFilter.Id, reconciliationFilter.FilterLabel, true)">{{ 'Copy' | translate }}</a></li>
                                            </ul>
                                        </li>
                                    </ul>
                                </div>
                            </div>

                            <div class="P0 col-md-12 col-sm-12 MB2">
                                <div class="filterHeaddingWrapper P0 col-md-12 col-sm-12" ng-click="HomeSearchGrid.iscustom= !HomeSearchGrid.iscustom">
                                    <span class="col-md-3 col-sm-3"> 
                                        <i class="fa fa-angle-down downArrow" ng-show = "HomeSearchGrid.iscustom"></i>
                                        <i class="fa fa-angle-right downArrow" ng-show = "!HomeSearchGrid.iscustom"></i>
                                    </span>
                                    <p class="filterHeadding text-uppercase T3 col-md-8 col-sm-8 P0">{{ 'Custom_Search' | translate }}</p>
                                </div>
                                <div class="homeSearchFilterListwrapper P0 col-md-12 col-sm-12 filterWrapper" ng-show = "HomeSearchGrid.iscustom"
                                    style="margin-bottom: 100px;">
                                    <ul class="homeSearchFilterList P0 col-md-12 col-sm-12">
                                        <li class="P0 col-md-12 col-sm-12" ng-repeat="customFilter  in HomeSearchGrid.filterList.FilterType.Custom | orderBy:'FilterLabel' | filter:HomeSearchGrid.sidebarSearch" 
                                            ng-mouseover="show = true" ng-mouseleave="show = false"
                                            ng-class="{'activeFilter' : HomeSearchGrid.isfilterActive(customFilter.Id, 'Custom')}">
                                            <span class="col-md-3 col-sm-3 text-center"> <i class="fa fa-square squaretrans"></i></span>
                                            <i class="bp-icons bp-font-14 bp-blue-font report-scheduling-cal-icon-custom" ng-if="customFilter.ScheduleReportObj.IsReportScheduled" 
                                                title="{{HomeSearchGrid.getScheduledReportTooltip(customFilter)}}" data-toggle="tooltip"    
                                                ng-class="{'bp-white-font': HomeSearchGrid.isfilterActive(customFilter.Id, 'Custom')}">calendarFilled</i>
                                            <p class="text-uppercase T3 col-md-7 col-sm-7 P0"  
                                            ng-click="HomeSearchGrid.applySavedFilter(customFilter, 'Custom')"><!-- data-original-title="{{customFilter.FilterLabel}}" data-toggle="tooltip" removed 7th march, 2017 by kajal #1587 -->
                                                {{customFilter.FilterLabel}}
                                            </p>
                                            <span class="col-md-1 col-sm-1 P0 dropup  dropdown-toggle" data-toggle="dropdown" role="button" ng-show="show">
                                                <i class="fa fa-cog PT5  "></i>
                                            </span>
                                             <ul class="dropdown-menu" role="menu">
                                                <li>
                                                    <a class="T7" ng-class="{'bp-disabled': customFilter.ScheduleReportObj.showLoading}" ng-if="HomeSearchGrid.reportScheduling.isReportSchedulingActionAvailable() && customFilter.ScheduleReportObj.IsReportSchedulable" ng-click="HomeSearchGrid.scheduleReport(customFilter)">{{(customFilter.ScheduleReportObj.IsReportScheduled ? 'Edit_report_scheduling' : 'Report_scheduling') | translate }}</a>
                                                    <i class="spinner-loading-icon" ng-if = "customFilter.ScheduleReportObj.showLoading">
                                                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="200px" height="200px" viewBox="0 0 100 100" preserveAspectRatio="xMidYMid">
                                                            <circle cx="50" cy="50" fill="none" stroke="#3384FF" stroke-width="10" r="35" stroke-dasharray="164.93361431346415 56.97787143782138">
                                                                <animateTransform attributeName="transform" type="rotate" repeatCount="indefinite" dur="1s" values="0 50 50;360 50 50" keyTimes="0;1"></animateTransform>
                                                            </circle>
                                                        </svg>
                                                    </i>
                                                </li>
                                                <li ng-click="HomeSearchGrid.deleteScheduledReport(customFilter.Id, 'Custom')" ng-if="HomeSearchGrid.reportScheduling.isReportSchedulingActionAvailable() && customFilter.ScheduleReportObj.IsReportScheduled">
                                                    <a  class="T7" >{{ 'Delete_report_scheduling' | translate }}</a>
                                                </li>
                                                <li><a class="T7" ng-click="HomeSearchGrid.renameOrCopyCustomFilter(customFilter.Id, customFilter.FilterLabel, false)">Rename</a></li>
                                                <li><a class="T7" ng-click="HomeSearchGrid.confirmDelete(customFilter.Id, customFilter.FilterLabel)">Delete</a></li>
                                            </ul>
                                        </li>
                                    </ul>
                            </div>
                            </div>
                        </div> 
                    </div>
                    <!-- Side panel End Here -->
    
    
    
    
    
                    <!-- center Div start here -->
                    <div class="col-lg-10 col-md-9 col-sm-9 contentDiv">
                        <div>
                            <div class="row M0 top_row">
                                <div class="col-md-9 col-sm-10 homeSearchTopBar">
                                    <div class="homeSearchTopBarContaint row">  
                                        <div class="col-md-9 col-sm-12">
                                            <img class="starimg pull-left" src="{!Application_Images}/star.png" 
                                                ng-show="HomeSearchGrid.currentActiveFilter.Type == 'favorite'"
                                                ng-click="HomeSearchGrid.markFilterFav(HomeSearchGrid.currentActiveFilter.Id, false)"/>
                                            <i class="fa fa-star pull-left"
                                                ng-show="HomeSearchGrid.currentActiveFilter.Type != 'favorite' && HomeSearchGrid.isFilterMatched"
                                                ng-click="HomeSearchGrid.markFilterFav(HomeSearchGrid.currentActiveFilter.Id, true)"></i>
                                            <p class="T3 text-uppercase pull-left" ng-show="!HomeSearchGrid.isFilterMatched">{{ 'Label_Search' | translate }}</p>
                                            <p class="T3 text-uppercase pull-left" ng-show="HomeSearchGrid.isFilterMatched">{{HomeSearchGrid.currentActiveFilter.Name}}</p>
                                            <i class="fa fa-question-circle pull-left" data-original-title="Search" data-toggle="tooltip"
                                                data-placement="right"></i>
                                            <div class="col-md-3 col-sm-3 homeSearchTopSaveBtn"> 
                                            <button class="btn C11 T4 active_btn" ng-click="HomeSearchGrid.saveAsAction()"
                                                ng-class="{'grayBtn': HomeSearchGrid.isFilterMatched, 'addmoreinfoBtn' : !HomeSearchGrid.isFilterMatched} " 
                                                ng-if="!HomeSearchGrid.isFilterMatched"
                                                style="margin: auto; width: 80%;" tabindex="112">{{ 'Save_As' | translate }}</button>
                                            </div>
                                            <p class="homeSearchcriteriaerror T3"
                                                ng-show="!HomeSearchGrid.isFilterMatched"> {{ 'New_Search_criteria_has_not_been_saved' | translate }}</p>
                                        </div> 
                                        
                                    </div>
                                
                                </div>
        
                                <div class="rightHeaderIcon pull-right col-md-3 col-sm-2">
                                            <div class="pull-right P0  homeSearchTopBarIcon">
                                                <i class="fa fa-pie-chart" ng-click = "HomeSearchGrid.showSummarySidePanel()"></i>
                                                <i class="fa fa-print" ng-click = "HomeSearchGrid.showExportSidePanel()"></i>
                                            </div>
                                        </div>
                                   
        
                            </div>
                            <!-- filter Div start here -->
                            <div class="row M0 filter_row">
                                    <div  ng-class="{'searchBoxMultiselectWrappereNew': HomeSearchGrid.isSidePanelToggle, 'searchBoxMultiselectWrappereNewDiv' : !HomeSearchGrid.isSidePanelToggle} ">
                                        <div class="searchBoxMultiselectWrappere pull-left">
                                            <div class="searchBoxMultiselect pull-left" ng-click="HomeSearchGrid.showFilterForm()">
                                                <div class="searchCriteriaWrapper"
                                                    ng-mouseover="showMore = false" ng-mouseleave="showMore = true">
                                                    <div class="searchCriteria pull-left" ng-repeat="filter in HomeSearchGrid.searchCriteriaJson">
                                                        <label class="T3 pull-left" ng-if="filter.Label != undefined && filter.Label != null && filter.Label != ''">{{filter.Label}}:</label>
                                                        <span class="T7 filterValue" data-original-title="{{HomeSearchGrid.getFilterValue(filter.Value)}}" data-toggle="tooltip" ng-bind="HomeSearchGrid.getFilterValue(filter.Value)">
                                                        </span>
                                                        <span class="crossIcon" ng-click="HomeSearchGrid.removeFilterCriteria($event, filter.Key, $index)">&#10006;</span>   
                                                    </div>
                                                </div>
                                                <span class="moreState" ng-show="HomeSearchGrid.isDisplayMore() && showMore">{{ 'More' | translate }} ...</span>
                                                <span class="pull-right dropdownicon">
                                                    <i class="fa fa-caret-down"></i>
                                                </span>
                                            </div>
                                            <div class="pull-left searchBoxIcondiv">
                                                <i class="fa fa-search"></i>
                    
                    
                                            </div>
                                            <c:HomeSearchFilterForm />
                                        </div>
                                        <div class="sortingDiv" ng-if = "!HomeSearchGrid.isLoadGridData">
                                            <span class="T3 pull-left" style="margin-top: 3px;">{{ 'Sorting' | translate }}:</span>
                                            <div class="pull-left">
                                                <div class="sorting-badge pull-left" ng-repeat="sort in HomeSearchGrid.searchResultPageSortAttrsJSON.Sorting">
                                                    <label>
                                                        <i class="fa fa-chevron-up" ng-if="sort.SortDirection == 'ASC'"></i>
                                                        <i class="fa fa-chevron-down" ng-if="sort.SortDirection == 'DESC'"></i>
                                                    </label>  
                                                    <span class="T7" style="font-size:12px; color: #fff;">
                                                        <span ng-if="HomeSearchGrid.LabelChangeMap[sort.FieldLabel]">{{HomeSearchGrid.LabelChangeMap[sort.FieldLabel]}}</span>
                                                        <span ng-if="!HomeSearchGrid.LabelChangeMap[sort.FieldLabel]">{{sort.FieldLabel}}</span>
                                                    </span>
                                                    <span class="crossIcon" ng-click="HomeSearchGrid.removeSortCriteria($index)">&#10006;</span>   
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="extraBackDropDiv" ng-class="{'extraBackDropDivHide': HomeSearchGrid.isSidePanelToggle, 'extraBackDropDiv' : !HomeSearchGrid.isSidePanelToggle} "></div>
                                    
                                    <!-- Side Panel Div start here -->
                                    <div class="rightSidePanel"  ng-class="{'summaryDivShow': HomeSearchGrid.isSidePanelToggle, 'summaryDivHide' : !HomeSearchGrid.isSidePanelToggle} " >
                                        <div class = "summaryInner" ng-show = "HomeSearchGrid.isSidePanelToggle">
                                         <div class = "summaryWrapper" ng-show = "HomeSearchGrid.summaryDiv">
                                            <div  class="summaryHeader col-md-12 col-sm-12">
                                                <p class="col-md-12 col-sm-12 P0 BBW">
                                                    <i class="fa fa-pie-chart F25 color727983 pull-left summaryIcon"></i>
                                                    <span class="summaryHeadding T3 pull-left">{{ 'Summary_Total' | translate }}</span>
                                                    <i class="fa fa-times pull-right color727983 CP F20 PB5 closeSummary" aria-hidden="true" 
                                                        ng-click = "HomeSearchGrid.hideSidePanel()"></i>
                                                    <!-- <i class="fa fa-cloud-upload pull-right color727983 CP F20 PB5 closeSummary" aria-hidden="true" 
                                                        ng-show="HomeSearchGrid.isFilterMatched"
