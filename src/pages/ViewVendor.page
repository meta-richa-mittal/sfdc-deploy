<apex:page standardStylesheets="false" showChat="false" showHeader="false" sidebar="false" docType="html-5.0"  controller="GlobalController">
    <div class="fadein" ui-view="EditVendor" autoscroll='false'></div>
    <c:VendorOrderCustomFormat />
    <div>
        <route-loading-indicator></route-loading-indicator>
        <div ng-cloak="ng-cloak" ng-if ="ViewVendor.ShowContent">  <!-- ng-if="!isRouteLoading" -->
            <div class="LoadingDiv" ng-show="ViewVendor.isLoading">
                <div class="loadingIconContainer">
                    <c:BPLoading />
                </div>
            </div>
        <div class="content">
        <div class="col-lg-2 col-md-2 sidePanel ">
            <div class="sidepaneluserinfo row">
                <div class="userNameblock userNameblock_Vendor_ML0">
                    <p class="T3"> 
                    <span> {{ViewVendor.VendorInfo.VendorDetailRec.VendorName}}</span>
                    </p>
                    <p class="customerNumber">{{ViewVendor.VendorInfo.VendorDetailRec.VendorCode}}</p>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="row statusRow">
                <span class="userStatus userStatus_Vendor_ML0" ng-show="ViewVendor.VendorInfo.VendorDetailRec.Active == true" >{{ 'Label_Active' | translate }}</span>
                <span class="userStatus userStatus_Vendor_ML0" style="color:#E80000;" ng-show="ViewVendor.VendorInfo.VendorDetailRec.Active == false" >{{ 'Label_Deactive' | translate }}</span>
            </div>
            
            
            <div class="leftPanelLinks">
                <a class="row T10 mainSectionLink" ng-class="{'active' : ViewVendor.activeSidepanelink =='#InfoSection'}" 
                    href="#InfoSection" ng-click="ViewVendor.sidepanelLink($event, 'Info')"  ><span class="alphabetLetter">{{ 'A' | translate }}</span>{{ 'Page_Section_Info' | translate }}</a>
                <a class="row T10 subSectionLink" href="#InfoSection" ng-click="ViewVendor.sidepanelLink($event, 'Info')">
                        <span class="alphabetLetter"></span>{{ 'Page_Section_General' | translate }}</a>    
                <a class="row T10 mainSectionLink" ng-class="{'active' : ViewVendor.activeSidepanelink =='#RelatedSection'}" 
                    href="#RelatedSection" ng-click="ViewVendor.sidepanelLink($event, 'Related')" ><span class="alphabetLetter">{{ 'B' | translate }}</span>{{ 'Page_Section_Related' | translate }}</a><!-- <span class="alphabetLetter">C</span> -->
                <a class="subSectionLink row T10" href="#RelatedSection_PurchaseOrder_Block" ng-click="ViewVendor.sidepanelLink($event, 'Related')">
                    <span class="alphabetLetter"></span>{{ 'Vendor_Order_Types' | translate }}</a>
                <a class="subSectionLink row T10" href="#RelatedSection_ActiveOrder_Block" ng-click="ViewVendor.sidepanelLink($event, 'Related')"
                    ng-show="GroupOnlyPermissions['Vendor order'].view">
                    <span class="alphabetLetter"></span>{{ 'Active_Orders' | translate }}</a>
                <a class="row T10 subSectionLink" href="#RelatedSection_Contact_Block" ng-click="ViewVendor.sidepanelLink($event, 'Related')">
                    <span class="alphabetLetter"></span>Contacts &amp; Shipping Addresses</a>
                <a class="row T10 subSectionLink" href="#RelatedSection_Product_BLock" ng-click="ViewVendor.sidepanelLink($event, 'Related')">
                    <span class="alphabetLetter"></span>{{ 'Label_Products' | translate }}</a>   
            </div>
            
        </div>
        <div class="col-lg-10 col-md-10 mainSection"> <!-- Scroll Issue: style="margin-top: 98px;": Removed By Pooja-->
            <div class=" sectionHeader row fixedRow" >
                <div class="col-lg-6 col-md-6" ng-show="ViewVendor.activeSidepanelink =='#InfoSection'">
                    <div class="fixedHeaderText">
                        <span class="headerlabel">
                        <i class="fa fa-angle-right" style="width:10px; padding: 0 3px;"
                            ng-show="!ViewVendor.displaySections.Info"
                            ng-Click="ViewVendor.displaySections.Info = true">
                        </i> 
                        <i class="fa fa-angle-down" style="width:10px;"
                            ng-show="ViewVendor.displaySections.Info"
                            ng-Click="ViewVendor.displaySections.Info = false">
                        </i> 
                        <span class="divider">|</span>
                        <span class="T2">{{ 'A' | translate }} {{ 'Page_Section_Info' | translate }}</span>
                        </span>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6" ng-show="ViewVendor.activeSidepanelink =='#RelatedSection'">
                    <div class="fixedHeaderText">
                        <span class="headerlabel">
                        <i class="fa fa-angle-right" style="width:10px; padding: 0 3px;"
                            ng-show="!ViewVendor.displaySections.Related"
                            ng-Click="ViewVendor.displaySections.Related = true">
                        </i> 
                        <i class="fa fa-angle-down" style="width:10px;"
                            ng-show="ViewVendor.displaySections.Related"
                            ng-Click="ViewVendor.displaySections.Related = false">
                        </i> 
                        <span class="divider">|</span>
                        <span class="T2">{{ 'B' | translate }} {{ 'Page_Section_Related' | translate }}</span><!-- C {{ 'Page_Section_Related' | translate }} -->
                        </span>
                    </div>
                </div>
            
                <div class="col-lg-6 col-md-6 fixedContent">
                    <div class="fa-icons">
                    <i class="fa fa-refresh fa-2x fa-fw"
                                    ng-class="{'fa-spin' : ViewVendor.isrefresh}"
                                    ng-click="ViewVendor.refreshViewVendor();"></i></div>
                    <button type="button" class="flatBtn turnUppercase"  ng-class="{'permissionDisabledButton' : !GroupOnlyPermissions['Vendors'].enabled}" ng-click="ViewVendor.editVendor()" ng-disabled="!GroupOnlyPermissions['Vendors'].enabled">{{ 'Edit_Label' | translate }}</button>
                    <div class="col-lg-7 col-md-6 searchToAdd pull-right viewVendorSearchBox">
                        <div class="input-group">
                            <div class="input-group-btn">
                                <button type="button" class="btn C5 LabelBtn">
                                <span class="T5 text-uppercase" style="color: #939CA9;">{{ 'Label_To' | translate }}:</span>
                                <span style="color: #FFFFFF;">{{ViewVendor.selectedItem}}</span>
                                </button>
                                <button type="button" class="btn C5 dropdown-toggle caretBtn" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <span class="caret"></span>
                                <span class="sr-only">{{ 'Label_Toggle_Dropdown' | translate }}</span>
                                </button>
                                <ul class="dropdown-menu">
                                    <li><a href="#InfoSection" ng-click="ViewVendor.dropDownItem($event, 'Info')">{{ 'Page_Section_Info' | translate }}</a></li>
                                    <li><a href="#RelatedSection" ng-click="ViewVendor.dropDownItem($event, 'Related')">{{ 'Page_Section_Related' | translate }}</a></li>
                                    <li role="separator" class="divider"></li>
                                    <li><a class="turnUppercase">{{ 'Label_Advanced' | translate }} {{ 'Label_Search' | translate }}</a></li>
                                </ul>
                            </div>
                            <div class="inner-addon right-addon">
                                <i class="fa fa-search fa-flip-horizontal"></i>
                                <c:SearchToAddHtml />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="InfoSection" >
                <div class="main_section row main_section_margin_top" ng-show="ViewVendor.displaySections.Info">
                    <p class="actionRow T2">
                        <span class="sectionText">{{ 'Page_Section_General' | translate }}</span>
                        <span class="ActionIcons">
                        <i class="fa fa-question-circle" data-toggle="tooltip" title="{{ViewVendor.helpText['Info']}}"></i>
                        </span>
                    </p>
                    <div class="row rowGeneral" >
                        <div class="col-md-5 col-lg-5 subsectionGeneral_rightonly">
                            <div  class="row">
                                <div class="col-md-7 col-lg-7" >
                                    <div class="row">
                                        <div class="col-md-12 col-lg-12">
                                            <div class="T1">
                                                <span> {{ViewVendor.VendorInfo.VendorDetailRec.VendorName}}</span>
                                            </div>
                                            <div class="T7">
                                                <span>
                                                {{ViewVendor.VendorInfo.VendorDetailRec.VendorAccountNumber}}
                                                </span>
                                            </div>
                                            <div class="T7">
                                                <span>
                                                {{ViewVendor.VendorInfo.VendorDetailRec.VendorCode}}
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row" >
                                <div class="col-md-7 col-lg-7 non_favorite" >
                                    <div class="social_icon_row label_favorite" ng-if= "ViewVendor.VendorInfo.VendorDetailRec.TwitterLink != '' || ViewVendor.VendorInfo.VendorDetailRec.FacebookLink != '' || ViewVendor.VendorInfo.VendorDetailRec.LinkedInLink != ''">
                                        <span>
                                            <a href="{{ViewVendor.VendorInfo.VendorDetailRec.TwitterLink}}" target="_blank" ng-if="ViewVendor.VendorInfo.VendorDetailRec.TwitterLink != ''">
                                                <i class="fa fa-twitter-square icon_tweet"></i>
                                            </a>
                                        
                                        </span>
                                        <span>
                                            <a href="{{ViewVendor.VendorInfo.VendorDetailRec.FacebookLink}}" target="_blank" ng-if="ViewVendor.VendorInfo.VendorDetailRec.FacebookLink != ''">
                                                <i class="fa fa-facebook-square icon_fb"></i>  
                                            </a>
                                        
                                        </span>
                                        <span>
                                            <a href="{{ViewVendor.VendorInfo.VendorDetailRec.LinkedInLink}}" target="_blank" ng-if="ViewVendor.VendorInfo.VendorDetailRec.LinkedInLink != ''">
                                                <i class="fa fa-linkedin-square icon_in"></i>  
                                            </a>
                                        
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="general_info_customer">
                                <div class="row" ng-if="ViewVendor.VendorInfo.VendorDetailRec.PreferredEmail != '' ">

                                    <div class="col-lg-5 col-md-5  customer_general_column">
                                        <span style="padding-top:10px;">    
                                            <img class="favoriteStarImg" src="{!Application_Images}/star.png" ng-if="ViewVendor.VendorInfo.VendorDetailRec.PreferredEmail != '' "/>
                                            <label class="T7" for="labelFirstName"
                                                ng-show="ViewVendor.VendorInfo.VendorDetailRec.PreferredEmail == 'OtherEmail'"><!-- ng-class="{ non_favorite: ViewVendor.VendorInfo.VendorDetailRec.PreferredEmail == ''}" -->
                                                 {{ 'Vendor_Other_Email' | translate }}
                                            </label>
                                            <label class="T7" for="labelFirstName"
                                                ng-show="ViewVendor.VendorInfo.VendorDetailRec.PreferredEmail == 'WorkEmail'">
                                                {{ 'Vendor_Work_Email' | translate }}
                                            </label>
                                        </span>
                                    </div>
                                    <div  class="col-lg-7 col-md-7 " > 
                                        <span class="blue T3_1 emailBreak"  ng-bind="ViewVendor.setPrefferedEmail()"> </span>
                                    </div>
                                </div>
                                <div class="row" ng-if="ViewVendor.VendorInfo.VendorDetailRec.PreferredPhone != '' ">

                                    <div class="col-lg-5 col-md-5 customer_general_column"> 
                                        <span style="padding-top:10px;">    
                                            <img class="favoriteStarImg" src="{!Application_Images}/star.png" ng-if="ViewVendor.VendorInfo.VendorDetailRec.PreferredPhone != '' "/>
                                            <label class="T7" for="labelFirstName"
                                                ng-show="ViewVendor.VendorInfo.VendorDetailRec.PreferredPhone == 'WorkNumber'">
                                                {{ 'Vendor_Work_Number' | translate }}
                                            </label>
                                            <label class="T7" for="labelFirstName"
                                                ng-show="ViewVendor.VendorInfo.VendorDetailRec.PreferredPhone == 'OtherPhone'">
                                                {{ 'Call_other' | translate }}
                                            </label> 
                                        </span>
                                    </div>
                                    <div  class="col-lg-7 col-lg-7" >
                                        <span class=" T3"  ng-bind="(ViewVendor.setPrefferedPhone())"> </span>
                                    </div>
                                </div>
                                <div class="row" ng-if="ViewVendor.VendorInfo.VendorDetailRec.PreferredSMS != '' ">

                                    <div class="col-lg-5 col-lg-5 customer_general_column">
                                        <span style="padding-top:10px;">    
                                            <img class="favoriteStarImg" src="{!Application_Images}/star.png" ng-if="ViewVendor.VendorInfo.VendorDetailRec.PreferredSMS != '' "/>
                                            <label class="T7" for="labelFirstName"
                                                ng-show="ViewVendor.VendorInfo.VendorDetailRec.PreferredSMS == 'FormattedWorkNumber'">
                                                {{ 'Vendor_Work_Number_SMS' | translate }}
                                            </label>
                                            <label class="T7" for="labelFirstName"
                                                ng-show="ViewVendor.VendorInfo.VendorDetailRec.PreferredSMS == 'FormattedOtherPhone'">
                                                {{ 'Sms_other' | translate }}
                                            </label>
                                        </span>
                                    </div>
                                    <div  class="col-lg-7 col-lg-7" >
                                        <span class="blue T3"  ng-bind="(ViewVendor.setPrefferedSMS());"> </span>
                                    </div>
                                </div>
                                <div class="row" ng-show="ViewVendor.VendorInfo.VendorDetailRec.Fax != ''">

                                    <div class="col-lg-5 col-lg-5 customer_general_column">
                                        <span style="padding-top:10px;">
                                                <label class="T7 non_favorite" for="labelFirstName">{{ 'Label_Send' | translate }} {{ 'Label_Fax' | translate }}</label>
                                        </span>
                                    </div>
                                    <div  class="col-lg-7 col-lg-7" >
                                        <span class="blue T3"  >{{ViewVendor.VendorInfo.VendorDetailRec.Fax}} </span>
                                    </div>
                                </div>
                                <div class="MoreSectionContent animate-showContent" >
                                    <div class="row " ng-show="ViewVendor.VendorInfo.VendorDetailRec.PreferredEmail != 'WorkEmail' && ViewVendor.VendorInfo.VendorDetailRec.WorkEmail != ''" >
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                <label class="T7 non_favorite" for="labelFirstName">{{ 'Vendor_Work_Email' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-md-7" >
                                            <span class="blue T3_1 emailBreak"  >{{ViewVendor.VendorInfo.VendorDetailRec.WorkEmail}} </span>
                                        </div>
                                    </div>
                                    <div class="row " ng-show="ViewVendor.VendorInfo.VendorDetailRec.PreferredEmail != 'OtherEmail' && ViewVendor.VendorInfo.VendorDetailRec.OtherEmail != ''" >
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                <label class="T7 non_favorite" for="labelFirstName">{{ 'Vendor_Other_Email' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-md-7" >
                                            <span class="blue T3_1 emailBreak"  >{{ViewVendor.VendorInfo.VendorDetailRec.OtherEmail}} </span>
                                        </div>
                                    </div>
                                    <div class="row "  ng-show="ViewVendor.VendorInfo.VendorDetailRec.PreferredPhone != 'WorkNumber'
                                        && ViewVendor.VendorInfo.VendorDetailRec.WorkNumber != ''">
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                <label class="T7 non_favorite" for="labelFirstName">{{ 'Vendor_Work_Number' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-md-7" >
                                            <span class="blue T3"  >{{ViewVendor.VendorInfo.VendorDetailRec.FormattedWorkNumber}} </span>
                                                                                                            <!-- | tel: Added By Pooja 4 Feb. 2017 -->

                                        </div>
                                    </div>
                                    <div class="row " ng-show="ViewVendor.VendorInfo.VendorDetailRec.PreferredSMS != 'WorkNumber' && ViewVendor.VendorInfo.VendorDetailRec.WorkNumber != ''">
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                <label class="T7 non_favorite" for="labelFirstName">{{ 'Vendor_Work_Number_SMS' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-md-7" >
                                            <span class="blue T3"  >{{ViewVendor.VendorInfo.VendorDetailRec.FormattedWorkNumber}} </span>
                                                                                                        <!-- | tel: Added By Pooja 4 Feb. 2017 -->

                                        </div>
                                    </div>
                                    <div class="row "  ng-show="ViewVendor.VendorInfo.VendorDetailRec.PreferredPhone != 'OtherPhone' &&
                                        ViewVendor.VendorInfo.VendorDetailRec.OtherPhone != ''">
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                <label class="T7 non_favorite" for="labelFirstName">{{ 'Call_other' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-md-7" >
                                            <span class="blue T3"  >{{ViewVendor.VendorInfo.VendorDetailRec.FormattedOtherPhone}} </span>
                                                                                            <!-- | tel: Added By Pooja 4 Feb. 2017 -->

                                        </div>
                                    </div>
                                    <div class="row "   ng-show="ViewVendor.VendorInfo.VendorDetailRec.PreferredSMS != 'OtherPhone' &&
                                        ViewVendor.VendorInfo.VendorDetailRec.OtherPhone != ''">
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                <label class="T7 non_favorite" for="labelFirstName">{{ 'Sms_other' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-md-7" >
                                            <span class="blue T3"  >{{ViewVendor.VendorInfo.VendorDetailRec.FormattedOtherPhone}} </span>
                                                                                                <!-- | tel: Added By Pooja 4 Feb. 2017 -->

                                        </div>
                                    </div>
                                    <div class="row " ng-show="ViewVendor.VendorInfo.VendorDetailRec.Website != ''">
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                    <label class="T7 non_favorite" for="labelFirstName">{{ 'Label_Visit_Website' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-lg-7" >
                                            <span class="blue T3 normalText"  >
                                                <a ng-if="((ViewVendor.VendorInfo.VendorDetailRec.Website).startsWith('http://') || (ViewVendor.VendorInfo.VendorDetailRec.Website).startsWith('https://'))" href="{{ViewVendor.VendorInfo.VendorDetailRec.Website}}" target="_blank">{{ViewVendor.VendorInfo.VendorDetailRec.Website}}</a>
                                                <a ng-if="!((ViewVendor.VendorInfo.VendorDetailRec.Website).startsWith('http://') || (ViewVendor.VendorInfo.VendorDetailRec.Website).startsWith('https://'))" href="http://{{ViewVendor.VendorInfo.VendorDetailRec.Website}}" target="_blank">{{ViewVendor.VendorInfo.VendorDetailRec.Website}}</a>
                                            </span>
                                        </div>
                                    </div>
                                    <div class="row" ng-if="ViewVendor.IsForeignExchangeCostingEnabled">
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                    <label class="T7 non_favorite" for="labelForeignVendor">
                                                        {{
                                                            'Foreign_Vendor' | translate
                                                        }}
                                                    </label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-lg-7" >
                                            <span class="blue T3 fx-vendor-view">
                                                {{ViewVendor.VendorInfo.VendorDetailRec.IsForeignVendor ? "YES" : "Local"}}
                                                <bp-flag currencyCode="{{ViewVendor.VendorInfo.VendorDetailRec.CurrencyCode}}"></bp-flag>
                                                {{ViewVendor.VendorInfo.VendorDetailRec.FormattedCurrencyCode}}
                                            </span>
                                        </div>
                                    </div>
                                    <div class="row " ng-show="ViewVendor.VendorInfo.VendorDetailRec.IsClaimsForServiceWork != '' 
                                                                || ViewVendor.VendorInfo.VendorDetailRec.IsFlooringCompany != ''
                                                                || ViewVendor.VendorInfo.VendorDetailRec.IsPartPurchases != ''
                                                                || ViewVendor.VendorInfo.VendorDetailRec.IsMerchandisePurchases != ''
                                                                || ViewVendor.VendorInfo.VendorDetailRec.IsUnitPurchases != ''
                                                                || ViewVendor.VendorInfo.VendorDetailRec.IsSubletPurchases != ''
                                                                || ViewVendor.VendorInfo.VendorDetailRec.IsWarrantyPlans != ''
                                                                || ViewVendor.VendorInfo.VendorDetailRec.IsFinanceCompany != ''
                                                                || ViewVendor.VendorInfo.VendorDetailRec.IsFinancingProducts != ''"> <!-- Edited By Kajal - #1225 - 10/14/2016 -->
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                    <label class="T7 non_favorite" for="labelFirstName">{{ 'Label_Products' | translate }} &amp; {{ 'Label_Services' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-lg-7" >
                                            <span class="blue T3"  >
                                                <ul style="margin-left: -25px;">
                                                    <li ng-if="ViewVendor.VendorInfo.VendorDetailRec.IsPartPurchases">{{ 'Part_Purchases' | translate }} </li>
                                                    <li ng-if="ViewVendor.VendorInfo.VendorDetailRec.IsMerchandisePurchases">{{ 'Merchandise_purchases' | translate }} </li>
                                                    <li ng-if="ViewVendor.VendorInfo.VendorDetailRec.IsUnitPurchases">{{ 'Unit_Purchases' | translate }}</li>
                                                    <li ng-if="ViewVendor.VendorInfo.VendorDetailRec.IsSubletPurchases">{{ 'Sublet_Purchases' | translate }} </li>
                                                    <li ng-if="ViewVendor.VendorInfo.VendorDetailRec.IsClaimsForServiceWork">{{ 'Claims_for_Service_Work' | translate }} </li>
                                                    <li ng-if="ViewVendor.VendorInfo.VendorDetailRec.IsWarrantyPlans">{{ 'Deal_Products' | translate }}</li> <!-- {{ 'Warranty_Plan_Sales_on_Unit_Deals' | translate }} -->
                                                    <li ng-if="ViewVendor.VendorInfo.VendorDetailRec.IsFlooringCompany">{{ 'Flooring_Company' | translate }}</li>
                                                    <li ng-if="ViewVendor.VendorInfo.VendorDetailRec.IsFinanceCompany">{{ 'Finance_Company' | translate }}</li>
                                                    <li ng-if="ViewVendor.VendorInfo.VendorDetailRec.IsFinancingProducts">{{ 'Financing_Products' | translate }}</li>
                                                </ul>
                                             </span>
                                        </div>
                                    </div>
                                    <div class="row " ng-show="ViewVendor.VendorInfo.VendorDetailRec.PartCategoryName != ''">
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                    <label class="T7 non_favorite" for="labelFirstName">{{ 'View_Part_Part_Category' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-lg-7" >
                                            <span class="blue T3"  >{{ViewVendor.VendorInfo.VendorDetailRec.PartCategoryName}} </span>
                                        </div>
                                    </div>
                                    
                                    <div class="row " ng-show="ViewVendor.VendorInfo.VendorDetailRec.MerchandiseCategoryName != ''">
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                    <label class="T7 non_favorite" for="labelFirstName">{{ 'Merchandise_category' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-lg-7" >
                                            <span class="blue T3"  >{{ViewVendor.VendorInfo.VendorDetailRec.MerchandiseCategoryName}} </span>
                                        </div>
                                    </div>
                                    
                                    <div class="row " ng-show="ViewVendor.VendorInfo.VendorDetailRec.PurchaseTaxLevelName != ''" >
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                    <label class="T7 non_favorite" for="labelFirstName">{{ 'Purchase_Tax_Level' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-lg-7" >
                                            <span class="blue T3"  >{{ViewVendor.VendorInfo.VendorDetailRec.PurchaseTaxLevelName}} </span>
                                        </div>
                                    </div>
                                    <div class="row" ng-show="ViewVendor.VendorInfo.VendorDetailRec.VendorTaxId" >
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                <label class="T7 non_favorite" for="labelFirstName">{{ 'Vendor_Tax_ID' | translate }}</label>
                                            </span>
                                        </div>
                                        <div class="col-lg-7 col-lg-7">
                                            <span class="blue T3">{{ViewVendor.VendorInfo.VendorDetailRec.VendorTaxId}} </span>
                                        </div>
                                    </div>
                                    <div class="row " ng-show="ViewVendor.VendorInfo.VendorDetailRec.SalsePriceLevelName != ''" >
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                    <label class="T7 non_favorite" for="labelFirstName">{{ 'Salse_Price_Level' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-lg-7" >
                                            <span class="blue T3"  >{{ViewVendor.VendorInfo.VendorDetailRec.SalsePriceLevelName}} </span>
                                        </div>  
                                    </div>
                                       <div class="row ">
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                    <label class="T7 non_favorite" for="labelFirstName">{{ 'Account_Type' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-lg-7" >
                                            <span class="blue T3"  >{{ViewVendor.VendorInfo.VendorDetailRec.AccountTypeName}} </span>
                                        </div>  
                                    </div>
                                    
                                    <div class="row " ng-show="ViewVendor.VendorInfo.VendorDetailRec.TaxExemptions" >
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span style="padding-top:10px;">
                                                    <label class="T7 non_favorite" for="labelFirstName">{{ 'Page_Section_TaxExemptions' | translate }}</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-lg-7" >
                                            <span class="blue T3"  >{{ViewVendor.VendorInfo.VendorDetailRec.TaxExemptions}} </span>
                                        </div>  
                                    </div>

                                    <!-- Start: VO Export format -->
                                    <div class="row ">
                                        <div class="col-lg-5 col-lg-5 customer_general_column">
                                            <span>
                                                <label class="T7 non_favorite" for="labelFirstName">VO Export Format</label>
                                            </span>
                                        </div>
                                        <div  class="col-lg-7 col-lg-7 vendor-export" >
                                            <span class="blue T3" ng-if="!ViewVendor.VendorInfo.VendorDetailRec.DefaultExportFormatName">Not Assigned</span>
                                            <span class="blue T3"  ng-if="ViewVendor.VendorInfo.VendorDetailRec.DefaultExportFormatName">{{ViewVendor.VendorInfo.VendorDetailRec.DefaultExportFormatName}}</span>
                                            <bp-ng-button name="" tool-tip-title="Edit" font-name="edit" font-class="bp-font-16 bp-dark-grey1-font" class-value="c-icon-btn" action="ViewVendor.exportVendorOrderModal()" disabled-value="" is-image-include="true" />
                                        </div>  
                                    </div>


                                    <div ng-show="ViewVendor.showExportConfirmationModal">
                                        <modal-dialog id="{{ViewVendor.ConfirmationModel.id}}" modal-css="ViewVendor.ConfirmationModel.modalCss" hide-close-icon="ViewVendor.ConfirmationModel.hideCloseIcon" close-icon-color="ViewVendor.ConfirmationModel.closeIconColor" show-dialog="ViewVendor.ConfirmationModel.showDialog" heading-text="ViewVendor.ConfirmationModel.headingText" 
                                        export-vendor-modal-alert-message="ViewVendor.exportVendorAlertMessage" message-text="ViewVendor.ConfirmationModel.messageText" 
                                            no-ok-btn="ViewVendor.ConfirmationModel.noOkBtn" payload="ViewVendor.ConfirmationModel.payload" is-alert="ViewVendor.ConfirmationModel.isAlert"
                                            custom-format-file-name="ViewVendor.customFormatFileName" cancel-btn-func="ViewVendor.ConfirmationModel.cancelBtnFunc()" >
                                            
                                            <div ng-if="(ViewVendor.ConfirmationModel.confirmationSource == 'ViewVendorExport')" class="bp-cell searchInputArea export-dropdown" ng-class = "{'showingOverlay' : ViewVendor.DropDownFocusId == 'ExportTypeInput'}" >
                                                <input  type="text" id="ExportTypeInput" class="searchInput" 
                                                ng-focus="ViewVendor.setFocusOnVendorExport('ExportTypeInput')"
                                                ng-blur  = "ViewVendor.setBlurOnVendorExport()"
                                                ng-disabled = "ViewVendor.isBtnSpinnerLoading"
                                                ng-keyup="ViewVendor.keyBoardavigation($event, ViewVendor.ExportTypeMap, 'ExportTypeDropDownDiv', 'ExportType')"
                                                ng-model="ViewVendor.VendorOrderExportType.Name" readonly="readonly" autocomplete="off" placeholder="Please select"/> 
                                                <i class="bp-icons bp-font-16 bp-blue-font bp-cursor-pointer stroke-3 downArrow dfDownArrow" ng-click = "ViewVendor.setFocusOnInput('ExportTypeInput')">arrowDown</i>
                                                <div class="searchResultOverlay" id="ExportTypeDropDownDiv">
                                                    <ul>
                                                        <li ng-repeat="exportType in sortedExportTypeMap = (ViewVendor.ExportTypeMap | orderBy :'Key') track by $index"
                                                            ng-mousedown="ViewVendor.setSelectedBlackpurlFormat(exportType)"
                                                            ng-class="{'selected-row': $index == ViewVendor.dropDownIndex}"
                                                            id="ExportType{{$index}}">
                                                            <div class="fullName text-capitalize">{{exportType.Name}}</div>
                                                        </li>  
                                                    </ul> 
                                                </div>
                                            </div>

                                            <div ng-if="!ViewVendor.isModifyEnabled" class="customFormat">
                                                <bp-ng-button name="Create a custom format" class-value="c-action-btn c-btn" disabled-value="ViewVendor.isBtnSpinnerLoading"
                                                action="ViewVendor.openCustomFormatModal()" font-name = "addSquare" font-class="bp-font-16 bp-dark-blue-font"></bp-ng-button>
                                            </div>
                                            <div ng-if="ViewVendor.isModifyEnabled" class="customFormat">
                                                <bp-ng-button name="Modify custom format" class-value="c-action-btn c-btn edit-icon" disabled-value="ViewVendor.isBtnSpinnerLoading"
                                                action="ViewVendor.openCustomFormatModal()" font-name = "edit" font-class="bp-font-16 bp-dark-blue-font bp-fw-100"></bp-ng-button>
                                            </div>

                                            <div class = "c-modal-window-footer">
                                                <span class = "c-primary-btn-container export-btn order-2">
                                                    <div ng-if="!ViewVendor.isBtnSpinnerLoading">
                                                        <bp-ng-button name="Save" class-value="c-ternary-btn c-btn-modal" action="ViewVendor.saveDefaultVOExportFormat()" disabled-value="ViewVendor.isExportButtonDisabled()" ></bp-ng-button>
                                                    </div>
                                                    <div ng-if="ViewVendor.isBtnSpinnerLoading">
                                                        <bp-ng-button name="" class-value="c-ternary-btn c-btn-modal btn-spinner-loading" disabled-value=""></bp-ng-button>
                                                        <i class="c-input-white-loading-icon">
                                                            <svg class="input-white-spiner" width="85px" height="85px" viewBox="0 0 66 66" xmlns="https://www.w3.org/2000/svg">
                                                                <circle class="input-white-spiner-path" fill="none" stroke-width="10" stroke-linecap="round" cx="33" cy="33" r="30"></circle> 
                                                            </svg>
                                                        </i>
                                                    </div>
                                                </span>
                                                <span class = "c-cancelBtn-btn-container order-0"> 
                                                    <bp-ng-button name="Cancel" class-value="c-cancel-btn c-btn-modal" action="ViewVendor.ConfirmationModel.cancelBtnFunc();" disabled-value="ViewVendor.isBtnSpinnerLoading" ></bp-ng-button>
                                                </span>
                                            </div>
                                        </modal-dialog>
                                    </div>
                                    <!-- End: VO Export format -->
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6 col-lg-6 subsectionGeneral" ng-if = " (ViewVendor.VendorInfo.VendorDetailRec.BillingCity != '' || ViewVendor.VendorInfo.VendorDetailRec.BillingPostalCode != '')">
                            <div class="row">
                                <div class=" col-lg-4 col-md-4 " style="padding-left:10px ;text-align:center;cursor:pointer" ng-if="(ViewVendor.VendorInfo.VendorDetailRec.BillingCity != '' || ViewCustomer.VendorInfo.VendorDetailRec.BillingPostalCode != '')">
                                   <div class="T4 round_btn" ng-click="ViewVendor.changeActiveAddress('BillingAddress')"
                                      ng-class="{C11: ViewVendor.activeAddressId == 'BillingAddress', C5: ViewVendor.activeAddressId != 'BillingAddress'}"> {{ 'Label_Billing' | translate }} </div>
                                </div>
                                <div ng-repeat="shippingAddress in ViewVendor.VendorInfo.VendorRelatedInfo.Contacts">
                                <div ng-if="shippingAddress.IsShippingAddress" class=" col-lg-4 col-md-4 " style="padding-left:10px ;text-align:center;cursor:pointer">
                                      <div class="T4 round_btn" ng-click="ViewVendor.changeActiveAddress(shippingAddress.Id)"
                                         ng-class="{C11: ViewVendor.activeAddressId == shippingAddress.Id, C5: ViewVendor.activeAddressId != shippingAddress.Id}">{{shippingAddress.LastName}}</div>
                                   </div>
                                </div>
                             </div>
                            <div class="row">
                                <div class="col-lg-12 col-md-12" style="padding:10px">
                                    <p class="T3">
                                        <b ng-show="ViewVendor.activeAddressId != 'BillingAddress'" >{{ViewVendor.activeAddress.LastName}}</b>
                                        <b ng-show="ViewVendor.activeAddressId == 'BillingAddress'" >{{ 'Billing_address' | translate }}</b>
                                    </p>
                                    <div>
                                        <div ng-show="ViewVendor.activeAddressId != 'BillingAddress'">
                                            <p class="T7">{{ViewVendor.activeAddress.ShippingAddressLine1}}</p>
                                            <p class="T7">{{ViewVendor.activeAddress.ShippingAddressLine2}}</p>
                                        </div>
                                        <div ng-show="ViewVendor.activeAddressId=='BillingAddress'">
                                            <p class="T7" >{{ViewVendor.VendorInfo.VendorDetailRec.AddressLine1}}</p>
                                            <p class="T7" >{{ViewVendor.VendorInfo.VendorDetailRec.AddressLine2}}</p>
                                        </div>
                                    </div>
                                    
                                </div>
                            </div>

                            <div class="row"  ng-if="!ViewVendor.viewMap">
                                <div class=" col-lg-4 col-md-4 viewMapBtn">
                                   <div class="C11 T4 round_btn"  ng-click="ViewVendor.handleViewMap()" > {{ 'Label_View_Map' | translate }} </div>
                                </div>
                             </div>
     
                             <div class="row mapBox" ng-if="ViewVendor.viewMap">
                                <div class="col-lg-12 col-md-12 mapBoxInnerDiv">
                                    <map zoom="12" center="37.7699298, -122.4469157" class="mapBoxMap" >
                                        <directions 
                                            draggable="false"
                                            panel="directions-panel" 
                                            travel-mode="DRIVING"
                                            origin="{{ViewVendor.OriginAddress}}"
                                            destination="{{ViewVendor.addressOnMap}}">
                                        </directions>
                                    </map>
                                </div>
                             </div>
                        </div>
                    </div>
                    
                    <div class="row viewVendor_PartPricingDetails" ng-if="ViewVendor.VendorInfo.VendorDetailRec.IsCalculatePartRetailPrice">
                        <div class="col-md-8" ng-if="ViewVendor.isSingleMarkup()">
                            {{ 'Part_retail_prices_are' | translate }} <span class="colorBlue text-lowercase">{{(ViewVendor.VendorInfo.VendorDetailRec.RetailEscalator.Markup1 > 0) ? ViewVendor.VendorInfo.VendorDetailRec.RetailEscalator.Markup1 + '% more' : ViewVendor.VendorInfo.VendorDetailRec.RetailEscalator.Markup1*-1 + '% less'}} </span> 
                            {{ 'than' | translate }} <span class="colorBlue"> {{ViewVendor.VendorInfo.VendorDetailRec.RetailBaseValue}}</span> {{ 'And_the_result' | translate }} 
							<span class="colorBlue"> {{ViewVendor.VendorInfo.VendorDetailRec.RetailRounding ? 'are' : 'are not'}} </span> {{ 'rounded' | translate }} 
                            <span ng-if="ViewVendor.VendorInfo.VendorDetailRec.RetailRounding"> {{ 'up_to_the_next' | translate }} </span> 
                            <span class="colorBlue" ng-if="ViewVendor.VendorInfo.VendorDetailRec.RetailRounding"> {{ViewVendor.VendorInfo.VendorDetailRec.RetailRoundTo}}</span> 
                            <span ng-if="ViewVendor.VendorInfo.VendorDetailRec.RetailRounding"> {{ 'cents' | translate }}</span> 
                        </div>

                        <div class="col-md-8" ng-if="!ViewVendor.isSingleMarkup()">
                            {{ 'Part_retail_prices_are' | translate }}
                            <span class="colorBlue text-lowercase"> tiered  </span> based on
                            <span class="colorBlue"> {{ViewVendor.VendorInfo.VendorDetailRec.RetailBaseValue}}</span>
                             {{ 'And_the_result' | translate }} 
							<span class="colorBlue"> {{ViewVendor.VendorInfo.VendorDetailRec.RetailRounding ? 'are' : 'are not'}} </span> 
                            {{ 'rounded' | translate }} 
                            <span ng-if="ViewVendor.VendorInfo.VendorDetailRec.RetailRounding"> {{ 'up_to_the_next' | translate }} </span> 
                            <span class="colorBlue" ng-if="ViewVendor.VendorInfo.VendorDetailRec.RetailRounding"> {{ViewVendor.VendorInfo.VendorDetailRec.RetailRoundTo}}</span> 
                            <span ng-if="ViewVendor.VendorInfo.VendorDetailRec.RetailRounding"> {{ 'cents' | translate }}</span> 
                        </div>
                    </div>
                </div>
            </div>
            <div class="sectionHeader row" style="margin-top: 48px"> <!-- Scroll Issue: style="margin-top: 48px": Added By Pooja  -->
                <div class="col-lg-4 col-lg-5">
                    <div class="fixedHeaderText">
                        <span class="headerlabel">
                        <i class="fa fa-angle-right" style="width:10px; padding: 0 3px;"
                            ng-show="!ViewVendor.displaySections.Related"
                            ng-Click="ViewVendor.displaySections.Related = true">
                        </i> 
                        <i class="fa fa-angle-down" style="width:10px;"
                            ng-show="ViewVendor.displaySections.Related"
                            ng-Click="ViewVendor.displaySections.Related = false">
                        </i> 
                        <span class="divider">|</span>
                        <span class="T2">{{ 'B' | translate }} {{ 'Page_Section_Related' | translate }}</span>
                        </span>
                    </div>
                </div>
            </div>
            <c:ViewVendorRelatedList /> 
            <div style="height:600px; background: #E6EBEC; margin: 0 -15px;" ></div>
           </div>
            </div>
        </div>
    </div>
    
