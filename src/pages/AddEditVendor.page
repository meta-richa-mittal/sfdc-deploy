                                                    max-table-rows="11"></rate-selector-table>
                                                    <div class = "rounded-container">And the results 
                                                    <div class="typeahead-wrapper">
                                                        <input class="bp-cursor-pointer" id="roundmsrpInput" tabindex="0" type="search" ng-blur = "VendorModal.isShowroundRetailDropDown = false" ng-focus="VendorModal.isShowroundRetailDropDown = true; " ng-model="VendorModal.VendorInfo.RetailRounding_Option.label" placeholder="{{'Placeholder_Search_Category' | translate }}" readonly = "readonly" />
                                                        <div class="hider" ng-class="{'active': VendorModal.isShowroundRetailDropDown}"></div>
                                                        <ul class="typeahead-list" ng-class="{'active' : VendorModal.isShowroundRetailDropDown }">
                                                                <li ng-repeat="roundingOptionRec in VendorModal.RetailRounding_Options" ng-mousedown = "VendorModal.changeDropDownValue(roundingOptionRec, 'roundedRetail')">
                                                                    <a tabIndex="0" href="" class="">{{roundingOptionRec.label}}</a>
                                                                </li>
                                                            </ul>
                                                        <i class="bp-icons bp-font-12 bp-blue-font bp-cursor-pointer stroke-5 bp-blue" ng-click="VendorModal.setFocusOnInput('roundmsrpInput')">arrowDown</i>
                                                        </div>
                                                        rounded <span ng-if = "VendorModal.VendorInfo.RetailRounding_Option.value">up to the next 
                                                        <numbers-only                              
                                                            input-model-value = "VendorModal.VendorInfo.RetailRoundTo" 
                                                            include-negative = "false"
                                                            include-zero = "true"
                                                            max-value="99"
                                                            min-value="0"
                                                        />
                                                        cents</span>
                                                    </div>
                                                </fieldset>
                                                </section>
                                            </div>
                                        </div>
                                    </div>
                                    
                                    <div class="addEditVendor_addmoreinfoBtn">
                                      <button type="button" class="btn C11 T4 addmoreinfoBtn" id="addmoreinfoBtnId"
                                             data-toggle="dropdown">{{ 'Label_Add_More_Information' | translate }}</button>
                                         <ul class="add_info_dropdown dropdown-menu keep_open">
