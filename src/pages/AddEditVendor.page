                                    <div class="modal-footer">
                                        <button type="button" id="confirmButton" class="btn btn-primary" ng-click="VendorModal.DeactiveVendor(true)">{{ 'Label_Yes' | translate }}</button>
                                        <button type="button" class="btn btn-default" ng-click="VendorModal.DeactiveVendor(false)">{{ 'Label_No' | translate }}</button>
                                    </div>
                                </div>
                            </div>
