    </head>
    <body>
        <div class="invoice-main-container c-pull-left">
            <c:StandardDocumentHeader headerText="{!if(isPrintReview, 'SERVICE ORDER', 'JOB WORKSHEET')}"
                shipToLabel="SHIP TO"
                orderLabel="ORDER#"
                isQRCodesOnServiceDocuments="{!isQRCodesOnServiceDocuments}"
                barcodeLabel="{!BarCodeQRCodeURL}"
            />
            <div class="main-section-container c-pull-left">
                <apex:outputPanel styleClass="c-pull-left M0" rendered="{!soWorkSheetObj.Services.size > 0}">
                    <apex:repeat value="{!soWorkSheetObj.Services}" var="serviceInfo">
                        <div class="c-pull-left merchandise-header">
                            <apex:outputPanel styleClass="c-pull-left M0 c-header">
                                <apex:outputPanel styleClass="c-pull-left M0" layout="none">
                                    <span class="c-pull-left M0 service-job-name">{!serviceInfo.Name}</span>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!serviceInfo.Salespersons != null}" layout="none">
                                    <apex:outputText styleClass="M0 sold-by-name" value="Sold by: {!serviceInfo.Salespersons}"/>
                                </apex:outputPanel>
                                <apex:outputText styleClass="c-pull-right M0 job-type" value="{!serviceInfo.JobType}"/>
                            </apex:outputPanel>
                        </div>
                        <div class="c-pull-left M0 unit-details-container table-container">
                            <table class="c-pull-left">
                                <col width="20%" /> 
                                <col width="15%" />
                                <col width="20%" />
                                <col width="20%" />
                                <col width="25%" /> 
                                <thead>
                                    <tr>
                                        <th>Job details</th>
                                        <th class="">Opened</th>
                                        <th class="">Appointment</th>
                                        <th class="">Invoiced</th>
                                        <th class="">Assigned to</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td class="c-bold"></td>
                                        <td> 
                                            <apex:outputText value="{!serviceInfo.CreatedDate}"/>
                                        </td>
                                        <td class="">
                                            <apex:outputText value="{!serviceInfo.Appointment}"/>
                                        </td>
                                        <td class="">
                                            <apex:outputText value="{!serviceInfo.Invoiced}"/>
                                        </td>
                                        <td class="">
                                            <apex:outputText value="{!serviceInfo.AssignedTo}"/>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>

                            <apex:outputPanel rendered="{!serviceInfo.TransactionType == 'Third-Party'}" layout="none">
                                <table class="c-pull-left M0 provider-table">
                                    <col width="10%" /> 
                                    <col width="90%" /> 
                                    <tbody>
                                        <tr>
                                            <td class="provider-info"><apex:outputText value="Provider:"></apex:outputText></td>
                                            <td class="bold-text-provider"><apex:outputText value="{!serviceInfo.ProviderName}"></apex:outputText></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </apex:outputPanel>

                            <table class="c-pull-left">
                        <col width="70%" /> 
                        <col width="15%" />
                        <col width="15%" /> 
                        <thead>
                            <tr>
                                <th>Unit details</th>
                                <th class="c-text-align-right">Mileage in</th>
                                <th class="c-text-align-right">Mileage out</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td class="c-bold"><apex:outputText value="{!serviceInfo.Unit.UnitDetail}"></apex:outputText></td>
                                <td class="c-text-align-right"> 
                                    <apex:outputText value="{0, number, ###}" rendered="{!serviceInfo.MileageIn != null}">
                                        <apex:param value="{!serviceInfo.MileageIn}" />
                                    </apex:outputText>
                                    <apex:outputText style="padding-left:2px;" value="{!serviceInfo.MileageType}" rendered="{!serviceInfo.MileageType != null}"></apex:outputText>
                                </td>
                                <td class="c-text-align-right">
                                    <apex:outputText value="{0, number, ###}" rendered="{!serviceInfo.MileageOut != null}">
                                        <apex:param value="{!serviceInfo.MileageOut}" />
                                    </apex:outputText>
                                    <apex:outputText style="padding-left:2px;" value="{!serviceInfo.MileageType}" rendered="{!serviceInfo.MileageType != null}"></apex:outputText>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="3"> 
                                    <div style="margin-left:15px">
                                        <apex:outputText rendered="{!and(serviceInfo.Unit.Type == 'STOCK', serviceInfo.Unit.StockNumber != null)}">
                                            [Stock#: {!serviceInfo.Unit.StockNumber}]&nbsp;
                                        </apex:outputText>
                                        <apex:repeat value="{!serviceInfo.Unit.UnitDataFieldObjList}" var="unitDataObj">
                                            <apex:outputText rendered="{!unitDataObj.Value != null}">
                                                [{!unitDataObj.Label}: {!unitDataObj.Value}]&nbsp;
                                            </apex:outputText>
                                        </apex:repeat>
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                            
                            <apex:outputPanel styleClass="c-pull-left M0 box-container" rendered ="{!(!isPrintReview || serviceInfo.ConcernList.size > 0)}">
                                <div class="c-box c-pull-left">
                                    <label>Concern </label>
                                    <ul class="c-pull-left M0">
                                        <apex:repeat value="{!serviceInfo.ConcernList}" var="concern">
                                            <li>{!concern}
                                            </li>
                                        </apex:repeat>
                                    </ul>
                                </div>
                            </apex:outputPanel>

                            <apex:outputPanel styleClass="c-pull-left M0 box-container" rendered ="{!(!isPrintReview || serviceInfo.CauseList.size > 0)}">
                                <div class="c-box c-pull-left">
                                    <label>Cause </label>
                                    <ul class="c-pull-left M0">
                                        <apex:repeat value="{!serviceInfo.CauseList}" var="cause">
                                            <li>{!cause}
                                            </li>
                                        </apex:repeat>
                                    </ul>
                                </div>
                            </apex:outputPanel>

                            <apex:outputPanel styleClass="c-pull-left M0 box-container" rendered ="{!(!isPrintReview || serviceInfo.CorrectionList.size > 0)}">
                                <div class="c-box c-pull-left">
                                    <label>Correction </label>
                                    <ul class="c-pull-left M0">
                                        <apex:repeat value="{!serviceInfo.CorrectionList}" var="correction">
                                            <li>{!correction}
                                            </li>
                                        </apex:repeat>
                                    </ul>
                                </div>
                            </apex:outputPanel>

                            <table class="c-pull-left">
                                <tbody>
                                    <tr>
                                        <td>    
                                            <table class="table pull-left">
                                                <apex:outputPanel rendered="{!!isHidePartNumbersOnDocuments}" layout="none">
                                                        <col width="10%" />
                                                </apex:outputPanel>
                                                <apex:outputPanel rendered="{!!isHidePartNumbersOnDocuments && isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                        <col width="40%" />
                                                </apex:outputPanel>
                                                <apex:outputPanel rendered="{!isHidePartNumbersOnDocuments && isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                        <col width="50%" />
                                                </apex:outputPanel>

                                                <apex:outputPanel rendered="{!!isHidePartNumbersOnDocuments && !isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                        <col width="60%" />
                                                </apex:outputPanel>
                                                <apex:outputPanel rendered="{!isHidePartNumbersOnDocuments && !isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                    <col width="70%" />
                                                </apex:outputPanel>
                                                <col width="10%" /> 
                                                <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                        <col width="10%" />
                                                </apex:outputPanel>
                                                <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                        <col width="10%" />
                                                </apex:outputPanel> 
                                                <col width="10%" /> 
                                                <col width="10%" /> 
                                                <thead>
                                                    <tr class="tableHeader T3 ">
                                                        <apex:outputPanel rendered="{!!isHidePartNumbersOnDocuments}" layout="none"><th>Item</th></apex:outputPanel>
                                                        <th>Description</th>
                                                        <th class="c-text-align-right">Qty</th>
                                                        <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                            <th class="c-text-align-right">MSRP
                                                                </th>
                                                        </apex:outputPanel>
                                                        <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                            <th class="c-text-align-right">Disc
                                                            </th>
                                                        </apex:outputPanel>
                                                        <th class="c-text-align-right">Price</th>
                                                        <th class="c-text-align-right">Total</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <apex:repeat value="{!serviceInfo.SOGridItems}" var="soGrid">
                                                        <apex:outputText rendered="{!soGrid.SOKH.Id != null}" >
                                                            <tr> 
                                                                <apex:outputPanel rendered="{!!isHidePartNumbersOnDocuments}" layout="none">
                                                                <td class="c-text-uppercase">Kit</td>
                                                                </apex:outputPanel>
                                                                <td>{!soGrid.SOKH.ItemDescription}</td>
                                                                <td></td>
                                                                <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                                <td></td>
                                                                </apex:outputPanel>
                                                                <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                                <td></td>
                                                                </apex:outputPanel>
                                                                <td></td>
                                                                <td  class="c-text-align-right">
                                                                    <apex:outputText value="{0, number, currency}" rendered="{!isPrintReview && serviceInfo.TransactionType == 'Customer'}">
                                                                        <apex:param value="{!soGrid.SOKH.Price}"/>
                                                                    </apex:outputText>
                                                                    <apex:outputText value="---" rendered="{!!isPrintReview || serviceInfo.TransactionType != 'Customer'}"/>
                                                                </td>
                                                            </tr>
                                                        </apex:outputText>
                                                        <apex:repeat value="{!soGrid.SOKH.SOLIList}" var="SoLi">
                                                            <tr >
                                                                <apex:outputPanel rendered="{!!isHidePartNumbersOnDocuments}" layout="none">
                                                                    <td class="c-text-uppercase">
                                                                        <apex:outputText rendered="{!soGrid.SOKH.Id == null}" value="{!SoLi.ItemCode}"></apex:outputText>
                                                                    </td> 
                                                                </apex:outputPanel>
                                                                <td >
                                                                    <apex:outputText rendered="{!soGrid.SOKH.Id == null || (SoLi.IsPart && isHidePartNumbersOnDocuments)}" value="{!SoLi.ItemDescription}"></apex:outputText>
                                                                    <apex:outputText rendered="{!soGrid.SOKH.Id != null && !(SoLi.IsPart && isHidePartNumbersOnDocuments)}" >
                                                                        <apex:outputText styleClass="c-text-uppercase" value="{!SoLi.ItemCode}"></apex:outputText> - <apex:outputText value=" {!SoLi.ItemDescription}"></apex:outputText>
                                                                    </apex:outputText>
                                                                </td>
                                                                <td class="c-text-align-right">
                                                                    <apex:outputText value="{0}" 
                                                                    rendered="{!(!soWorkSheetObj.IsHideHoursRateOnServiceDocuments || !SoLi.IsLabour)}">
                                                                        <apex:param value="{!SoLi.QtyNeeded}"/>
                                                                    </apex:outputText>
                                                                </td> 
                                                                <apex:outputPanel rendered="{!isIncludeMSRPAndDiscountOnPartLines}" layout="none">
                                                                    <td class="c-text-align-right"> 
                                                                        <apex:outputText value="{0, number, currency}" rendered="{!SoLi.IsPart && isPrintReview && serviceInfo.TransactionType == 'Customer'}">
                                                                            <apex:param value="{!SoLi.ActualRetailPrice}"/>
                                                                        </apex:outputText>
                                                                        <apex:outputText value="---" rendered="{!!isPrintReview || serviceInfo.TransactionType != 'Customer'}"/>
                                                                    </td>
                                                                </apex:outputPanel>
