                    <input id="jobStylingInput" class="c-input-search c-secondary-input job-styling" readonly="true" ng-focus="UserSettingModel.showOverlay = true" ng-blur="UserSettingModel.showOverlay = false" />
                    <span class="statusTitle {{UserSettingModel.ModalWindowPayload.ServiceJobStatusConfig.TextColor}} {{UserSettingModel.ModalWindowPayload.ServiceJobStatusConfig.BgColor}}" ng-click="UserSettingModel.setFocusOnInput('jobStylingInput')">label</span>
                    <span class = "bp-autocomplete-image-icon" ng-click="UserSettingModel.setFocusOnInput('jobStylingInput')">
                        <i class="bp-icons bp-font-14 bp-blue-font bp-cursor-pointer downArrow stroke-3 MT-3">arrowDown</i>
                    </span>
                    <div class="searchResultOverlay" ng-if="UserSettingModel.showOverlay">
                        <ul>
                            <li ng-repeat="styleItem in UserSettingModel.jobStylingList"
                                ng-mousedown="UserSettingModel.selectJobStatusStyle($index, UserSettingModel.ModalWindowPayload.index, styleItem.TextColor, styleItem.BgColor)" >
                                <span class="statusTitle {{styleItem.TextColor}} {{styleItem.BgColor}}" ng-class="{'selectedStyle': $index == UserSettingModel.ModalWindowPayload.selectedStyleIndex}">label
                                    <i ng-if="$index == UserSettingModel.ModalWindowPayload.selectedStyleIndex" class="bp-icons bp-font-24 bp-dark-grey1-font stroke-3 MT-3">check</i>
                                </span>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="c-form-field">
                <label class="job-status-label">Active</label>
                <label class="bp-switch margin8 bp-cursor-pointer">
                    <input ng-model="UserSettingModel.ModalWindowPayload.ServiceJobStatusConfig.IsActive" ng-click="UserSettingModel.toggleActiveServiceJob(true, UserSettingModel.ModalWindowPayload.index)" ng-checked="UserSettingModel.ModalWindowPayload.ServiceJobStatusConfig.IsActive" type="checkbox" />
                    <div class="bp-slider round"></div>
                </label>
            </div>
            <div class="active-jobs">
                <label>{{'Active_service_jobs_using_this_status' | translate}} = {{UserSettingModel.ModalWindowPayload.serviceJobsUsingThisStatus}}</label>
            </div>
        </div>
    </bp-ng-modal-window>
    <bp-ng-modal-window id-val="addEditProductType" mobal-window-payload="UserSettingModel.ModalWindowPayload" cancel-btn-action="UserSettingModel.closeAddEditProductType()" primary-btn-action="UserSettingModel.saveProductType()" disable-primary-action="UserSettingModel.isSaveProductTypeBtnDisabled()" class="edit-job-status add-edit-product-type">
        <div class="c-form-field-container">
            <div class="c-form-field">
                <label class="job-status-label">Product type</label>
                <input type="text" ng-model="UserSettingModel.ModalWindowPayload.ProductTypeConfig.Name" ng-class="{'red-border': UserSettingModel.ModalWindowPayload.isErrorWithProductTypeName}" ng-blur="UserSettingModel.validateAddEditProductType('Name')" class="c-input-search c-secondary-input status-name" placeholder="Enter name" />
            </div>
            <div class="c-form-field">
                <label class="job-status-label">Active</label>
                <label class="bp-switch margin8 bp-cursor-pointer">
                    <input ng-model="UserSettingModel.ModalWindowPayload.ProductTypeConfig.Active" ng-click="UserSettingModel.actionToggle('Active')" type="checkbox" />
                    <div class="bp-slider round"></div>
                </label>
            </div>
            <div class="c-form-field">
                <label class="job-status-label">Default</label>
                <label class="bp-switch margin8 bp-cursor-pointer">
                    <input ng-model="UserSettingModel.ModalWindowPayload.ProductTypeConfig.IsDefault" ng-click="UserSettingModel.actionToggle('IsDefault')" type="checkbox" />
                    <div class="bp-slider round"></div>
                </label>
            </div>
            <div class="c-form-field">
                <label class="job-status-label unit-details">Unit details</label>
            </div>
            <div class="c-form-field set-category-name-btn">
                <div ng-repeat="category in UserSettingModel.ModalWindowPayload.ProductTypeConfig.CategoryList">
                    <button ng-if="category.FieldValuesList.length > 0" class="btn btn-default" ng-class="{'btn-primary': UserSettingModel.ModalWindowPayload.selectedProductTypeCategoryIndex == $index}" ng-click="UserSettingModel.setActiveProductTypeCategoryIndex($index)">{{category.CategoryName}}</button>
                </div>
            </div>
            <div class="c-form-field">
                <div class="no-border">
                    <div class="form table-responsive price-level-table-container">
                        <div class="MB0 category-table">
                            <table class="bp-editable-table price-level-table MB20">
                                <col width="19%"/>
                                <col width="11%"/>
                                <col width="7%"/>
                                <col width="30%"/>
                                <col width="15%"/>
                                <col width="18%"/>
                                <thead>
                                    <tr>
                                        <th class="H300 bp-letter-capitalize line-height-medium">Field name</th>
                                        <th class="H300 bp-letter-capitalize line-height-medium">Field type</th>
                                        <th class="bp-letter-capitalize">Active?</th>
                                        <th class="bp-text-align-center bp-letter-capitalize"><span class="expose-on">Expose on: </span><span>Service document</span></th>
                                        <th class="bp-letter-capitalize">Deal document</th>
                                        <th class="bp-letter-capitalize">Order document</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="productTypeCategoryField in UserSettingModel.ModalWindowPayload.ProductTypeConfig.CategoryList[UserSettingModel.ModalWindowPayload.selectedProductTypeCategoryIndex].FieldValuesList track by $index" ng-init="productTypeCategoryFieldIndex = $index">
                                        <td>
                                            <span class="H300">{{productTypeCategoryField.Name}}</span>
                                        </td>
                                        <td>
                                            <span>{{productTypeCategoryField.FieldType}}</span>
                                        </td>
                                        <td>
                                            <label class="checkbox">
                                                <span class="chk_tick"
                                                    ng-class="{'chked': productTypeCategoryField.Active}"
                                                    ng-click="UserSettingModel.selectDeselectProductTypeCategoryFieldItem('Active', $index);">
                                                    <i class="fa fa-check-square chksqure"></i>
                                                </span>
                                           </label>
                                        </td>
                                        <td class="bp-text-align-center fourth-column">
                                            <label class="checkbox">
                                                <span class="chk_tick"
                                                    ng-class="{'chked': productTypeCategoryField.IncludeInSODocument, 'checkbox-disabled': !productTypeCategoryField.Active}"
                                                    ng-click="UserSettingModel.selectDeselectProductTypeCategoryFieldItem('IncludeInSODocument', $index);">
                                                    <i class="fa fa-check-square chksqure"></i>
                                                </span>
                                           </label>
                                        </td>
                                        <td>
                                            <label class="checkbox">
                                                <span class="chk_tick"
                                                    ng-class="{'chked': productTypeCategoryField.IncludeInDealDocument, 'checkbox-disabled': !productTypeCategoryField.Active}"
                                                    ng-click="UserSettingModel.selectDeselectProductTypeCategoryFieldItem('IncludeInDealDocument', $index);">
                                                    <i class="fa fa-check-square chksqure"></i>
                                                </span>
                                           </label>
                                        </td>
                                        <td>
                                            <label class="checkbox">
                                                <span class="chk_tick"
                                                    ng-class="{'chked': productTypeCategoryField.IncludeInUnitOrderDocument, 'checkbox-disabled': !productTypeCategoryField.Active}"
                                                    ng-click="UserSettingModel.selectDeselectProductTypeCategoryFieldItem('IncludeInUnitOrderDocument', $index);">
                                                    <i class="fa fa-check-square chksqure"></i>
                                                </span>
                                           </label>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </bp-ng-modal-window>
    <bp-ng-modal-window id-val="addEditStampDutyModal" mobal-window-payload="UserSettingModel.ModalWindowPayload" cancel-btn-action="UserSettingModel.closeModalWindow('addEditStampDutyModal')" primary-btn-action="UserSettingModel.saveStampDutyRecord()" disable-primary-action="UserSettingModel.isSaveStampDutyBtnDisabled()" class="add-edit-stamp-duty-modal-window">
        <div>
            <section class="section-container">
                <div class="individual-section">
                    <label class="H300 bp-lh-36 MB0">Name</label>
                    <input ng-model="UserSettingModel.ModalWindowPayload.StampDutyRec.Name" maxlength="60" class="c-input-search c-secondary-input status-name" ng-blur="UserSettingModel.validateAddEditStampDutyName()" ng-class="{'red-border': UserSettingModel.ModalWindowPayload.isErrorWithStampDutyName}" placeholder="Enter name" type="text" />
                </div>
                <div class="individual-section">
                    <label class="H300 bp-lh-36 MB0">Calculation method</label>
                    <div class="bp-cell searchInputArea" ng-class="{'showingOverlay' : UserSettingModel.showCalcMethodPickListValues }">
                        <input id="calculationMethod" autocomplete="off" class="bp-input-text searchInput" type="text"
                        ng-model="UserSettingModel.ModalWindowPayload.StampDutyRec.CalculationMethod" ng-blur="UserSettingModel.showCalcMethodPickListValues = false;"
                        ng-focus="UserSettingModel.showCalcMethodPickListValues = true;" readonly="true" />
                        <span>
                            <i class="bp-icons bp-font-14 bp-blue-font stroke-5 downArrow MT-3" ng-click="UserSettingModel.setFocusOnInput('calculationMethod')">arrowDown</i>
                        </span>
                        <div class="searchResultOverlay" id="calculationMethodDropDownDiv">
                            <ul>
                                <li ng-mousedown="UserSettingModel.selectCalculationMethod(calculationMethod)" 
                                ng-repeat="calculationMethod in UserSettingModel.PriceAndTax.calcMethodPickListValues">
                                    <div class="fullName">{{calculationMethod.Name}}</div>
                                </li>
                            </ul> 
                        </div>
                    </div>
                </div>
                <div class="individual-section">
                    <label class="H300 bp-lh-36 MB0">Minimum result</label>
                    <div class="doller-sign-container">
                        <span class="doller-sign">$</span>
                        <numbers-only id-value="minResult" class-value="bp-new-input" precision-length="2" force-precision-length = "true" ng-class-name="{'bp-redborder' : UserSettingModel.isValidateMinimum}" input-model-value="UserSettingModel.ModalWindowPayload.StampDutyRec.Minimum" blur-value="UserSettingModel.validationMinimum(UserSettingModel.ModalWindowPayload.StampDutyRec.Minimum, UserSettingModel.ModalWindowPayload.StampDutyRec.Maximum); " />
                    </div>
                </div>
                <div class="individual-section">
                    <label class="H300 bp-lh-36 MB0">Maximum result</label>
                    <div ng-class="{'doller-sign-container' : UserSettingModel.ModalWindowPayload.StampDutyRec.Maximum != '-unlimited-'}" >
                        <span class="doller-sign" ng-if="UserSettingModel.ModalWindowPayload.StampDutyRec.Maximum != '-unlimited-'">$</span>
                        <numbers-only id-value="maxResult" class-value="bp-new-input" precision-length="2" force-precision-length = "true" ng-class-name="{'bp-redborder' : UserSettingModel.isValidateMaxmum }" blur-value="UserSettingModel.validationMaximum(UserSettingModel.ModalWindowPayload.StampDutyRec.Minimum, UserSettingModel.ModalWindowPayload.StampDutyRec.Maximum);" input-model-value="UserSettingModel.ModalWindowPayload.StampDutyRec.Maximum" is-un-limited="true" />
                    </div>
                </div>
                <div class="individual-section">
                    <label class="dutiable-label">Dutiable values</label>
                    <div class="stamp-duty-rates-table">
                        <table class="bp-editable-table">
                            <col width="25%" />
                            <col width="25%" />
                            <col width="25%" />
                            <col width="25%" />
                            <thead>
                                <tr>
                                    <th>From</th>
                                    <th>To</th>
                                    <th>Rate</th>
                                    <th>Per</th>
                                </tr>
                            </thead>
                            <tbody class="no-border">
                                <tr ng-repeat="StampDutyTable in UserSettingModel.ModalWindowPayload.StampDutyRec.StampDutyTable">
                                    <td>{{StampDutyTable.from | currency}}</td>
                                    <td class="bp-first-letter-capitalize">
                                        <div ng-class="{'doller-sign-container' : StampDutyTable.to != '-unlimited-'}">
                                            <span class="doller-sign" ng-if="StampDutyTable.to != '-unlimited-'">$</span>
                                            <numbers-only class-value="bp-new-input" precision-length="2" force-precision-length = "true" input-model-value="StampDutyTable.to" 
                                                disabled-value="$index == 3 || UserSettingModel.ModalWindowPayload.StampDutyRec.CalculationMethod == 'Single rate'" 
                                                ng-class-name="{'bp-redborder' : StampDutyTable.isValidateTo == false}" 
                                                input-keyup-value="UserSettingModel.updateToValue(StampDutyTable.to, $index)" 
                                                is-un-limited="true" blur-value="UserSettingModel.validateToValue()" />
                                        </div>
                                    </td>
                                    <td>
                                        <div class="doller-sign-container" ng-if="$index == 1 && UserSettingModel.ModalWindowPayload.StampDutyRec.CalculationMethod == 'WA special rate' ">
                                            <input ng-disabled="$index == 1 && UserSettingModel.ModalWindowPayload.StampDutyRec.CalculationMethod == 'WA special rate'" 
                                                class="bp-new-input" value="Calculated" />
                                        </div>
                                        <div ng-if="!($index == 1 && UserSettingModel.ModalWindowPayload.StampDutyRec.CalculationMethod == 'WA special rate')">
                                            <numbers-only class-value="bp-new-input" precision-length="4" force-precision-length = "true" input-model-value="StampDutyTable.rate" 
                                               blur-value="UserSettingModel.blurDutiableValueRate($index)" include-percent-symbol="true" />
                                        </div>
                                    </td>
                                    <td>
                                        <div class="doller-sign-container">
                                            <span class="doller-sign">$</span>
                                            <numbers-only class-value="bp-new-input" precision-length="2" force-precision-length = "true" input-model-value="StampDutyTable.per" />
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

                <div class="toggle-buttons-section">
                    <div class="active-toggle">
                        <label class="H300">{{'Active' | translate}}</label>
                        <label class="bp-switch">
                            <input ng-model="UserSettingModel.ModalWindowPayload.StampDutyRec.IsActive" type="checkbox" ng-click="UserSettingModel.stampDutyActionToggle('isActive', $event)" />
                            <div class="bp-slider round"></div>
                        </label>
                    </div>
                    <div class="default-toggle">
                        <label class="H300">{{'Default' | translate}}</label>
                        <label class="bp-switch">
                            <input ng-model="UserSettingModel.ModalWindowPayload.StampDutyRec.IsDefault" type="checkbox" ng-click="UserSettingModel.stampDutyActionToggle('isDefault', $event)" />
                            <div class="bp-slider round"></div>
                        </label>
                    </div>
                </div>
            </section>
        </div>
    </bp-ng-modal-window>
</apex:page>