<apex:page standardStylesheets="false" showChat="false" showHeader="false" sidebar="false" Controller="PrintCODepositCtrl">
    <html ng-app="serviceWorksheetModule" >
        <head>
            <c:CSSFiles Application="{!Application}" Application_Images="{!Application_Images}" Assests="{!Assests}" />
            <c:JsFiles Application="{!Application}" Application_Images="{!Application_Images}" Assests="{!Assests}" />  
            <apex:stylesheet value="{!Application}/css/PrintPreviewStyle.css"/>
            <apex:stylesheet value="{!Application}/css/Printcss.css"/>
            <apex:stylesheet value="{!Application}/css/PrintCODeposits.css"/>
            
            <script>
                var serviceWorksheetModule = angular.module('serviceWorksheetModule', []);
                
                serviceWorksheetModule.filter('tel', function () {
                   return function (tel) {
                       return formatTel(tel);
                   };
                });
                
                serviceWorksheetModule.factory('serviceWorksheetService', function ($q) {
                    return {
                        getServiceWorksheetData : function (CoHeaderId) {
                            CoHeaderId = encodeString(CoHeaderId);
                            var deferred = $q.defer();
                            // make AJAX request to the remote service
                            Visualforce.remoting.Manager.invokeAction( '{!$RemoteAction.PrintCODepositCtrl.getPrintCODepositData}' ,CoHeaderId, 
                            function (result, event) {
                                if (event.type == 'exception') {
                                    deferred.reject(event.message);
                                } else {
                                    var responseData = decodeString(result);
                                    var find = '\'';
                                    var re = new RegExp(find, 'g');
                                    responseData = responseData.replace(re, '');
                                    if(responseData != ''){
                                        deferred.resolve(JSON.parse(responseData));
                                    } else {
                                        deferred.resolve(responseData);
                                    }
                                }
                            }, {
                                escape : true
                            });
                            return deferred.promise;
                        },
                        getPrintCODepositDataWithBrandingLocation : function (CoHeaderId,BrandingLocationId) {
                            CoHeaderId = encodeString(CoHeaderId);
                            BrandingLocationId = encodeString(BrandingLocationId);
                            var deferred = $q.defer();
                            // make AJAX request to the remote service
                            Visualforce.remoting.Manager.invokeAction( '{!$RemoteAction.PrintCODepositCtrl.getPrintCODepositDataWithBrandingLocation}' ,CoHeaderId,BrandingLocationId, 
                            function (result, event) {
                                if (event.type == 'exception') {
                                    deferred.reject(event.message);
                                } else {
                                    var responseData = decodeString(result);
                                    var find = '\'';
                                    var re = new RegExp(find, 'g');
                                    responseData = responseData.replace(re, '');
                                    if(responseData != ''){
                                        deferred.resolve(JSON.parse(responseData));
                                    } else {
                                        deferred.resolve(responseData);
                                    }
                        }
                            }, {
                                escape : true
                            });
                            return deferred.promise;
                        }
                        
                    }
                });
        
        
                
                serviceWorksheetModule.controller('ServiceWorksheetController', function (serviceWorksheetService, $scope,$timeout, $window) {
                    $scope.serviceWorksheetModal = {};
                    $scope.serviceWorksheetModal.isDataLoaded = false;
                    $scope.serviceWorksheetModal.LoadServiceWorksheetData = function()  {
                        
                        $scope.serviceWorksheetModal.CoHeaderId = "{!JSENCODE(URLENCODE($CurrentPage.parameters.id))}";
                        $scope.serviceWorksheetModal.brandingLocationId = "{!JSENCODE(URLENCODE($CurrentPage.parameters.brandingLocationId))}";
                        var CoHeaderId = $scope.serviceWorksheetModal.CoHeaderId;
                        $scope.serviceWorksheetModal.isHidePartNumbersOnDocuments = {!isHidePartNumbersOnDocuments};
                        $scope.serviceWorksheetModal.isHideKitDetailsOnDocuments = {!isHideKitDetailsOnDocuments};
                        
                        var responseData = {};
                        if($scope.serviceWorksheetModal.brandingLocationId) {
                        	   serviceWorksheetService.getPrintCODepositDataWithBrandingLocation(CoHeaderId,$scope.serviceWorksheetModal.brandingLocationId)
		                        .then(function (successfulServiceWorksheetData) {
		                            responseData = successfulServiceWorksheetData;
		                            
		                            $scope.serviceWorksheetModal.processResults(responseData);
		                            $scope.serviceWorksheetModal.isDataLoaded = true;
		                        },
		                        function (errorSearchResult) {
		                            responseData = errorSearchResult;
                        	});
                        } else {
                        serviceWorksheetService.getServiceWorksheetData(CoHeaderId)
                        .then(function (successfulServiceWorksheetData) {
                            responseData = successfulServiceWorksheetData;
                            
                            $scope.serviceWorksheetModal.processResults(responseData);
                            $scope.serviceWorksheetModal.isDataLoaded = true;
                        },
                        function (errorSearchResult) {
                            responseData = errorSearchResult;
                        }); 
                        }
                       
                    }
                    $scope.serviceWorksheetModal.printview = function() {
                        $timeout($window.print, 0);
                    }
                    $scope.serviceWorksheetModal.processResults = function(responseData){
                        
                        $scope.serviceWorksheetModal.serviceWorksheetData = responseData;
                        
                    }   
                    $scope.serviceWorksheetModal.isShowTotal = function() {
                        if($scope.serviceWorksheetModal.serviceWorksheetData.WorksheetTotal.TotalUnits != 0 ||
                            $scope.serviceWorksheetModal.serviceWorksheetData.WorksheetTotal.TotalTradeIns != 0 ||
                            $scope.serviceWorksheetModal.serviceWorksheetData.WorksheetTotal.TotalLienPayout != 0 ||
                            $scope.serviceWorksheetModal.serviceWorksheetData.WorksheetTotal.TotalWarranty != 0 ||
                            $scope.serviceWorksheetModal.serviceWorksheetData.WorksheetTotal.TotalSublet != 0 ||
                            $scope.serviceWorksheetModal.serviceWorksheetData.WorksheetTotal.TotalOtherProducts != 0 ||
                            $scope.serviceWorksheetModal.serviceWorksheetData.WorksheetTotal.TotalPart != 0 ||
                            $scope.serviceWorksheetModal.serviceWorksheetData.WorksheetTotal.TotalLabor != 0 ||
                            $scope.serviceWorksheetModal.serviceWorksheetData.WorksheetTotal.TotalFee != 0 ||
                            $scope.serviceWorksheetModal.serviceWorksheetData.WorksheetTotal.TotalShopSupplies != 0 ||
                            $scope.serviceWorksheetModal.serviceWorksheetData.WorksheetTotal.StampDuty != 0 ||
                            $scope.serviceWorksheetModal.serviceWorksheetData.WorksheetTotal.TotalFinanceProducts != 0) {
                                return true;
                            } else {
                                return false;
                            }
                    }  
                });
            </script>
