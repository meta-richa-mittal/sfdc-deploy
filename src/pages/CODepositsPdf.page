<apex:page showHeader="false" renderAs="pdf" sidebar="false" standardstylesheets="false"  applyHtmlTag="false" docType="html-5.0" applyBodyTag="false" controller="PrintCODepositCtrl">
    <head>
        <style>
            body {
                float: left;
                width: 100%;
                height: 100%;
                margin: 0;
                padding: 0px;
                font-family: sans-serif;
            }
            .header-section{
                margin-bottom: 9px;
                clear: both;
            }
            .c-pull-left {
                float: left;
            }
            .c-pull-right {
                float: right;
            }
            .store-info {
                text-align: right;
                width: 350px;
                
            }
            .store-info label {
                font-weight: bold;
                font-size: 11px;
                line-height: 11px;
                color: #000;
                text-transform: uppercase;
                margin-bottom: 5px;
                width: 100%;
            }
            .store-info p {
                text-align: right;
                width: 350px;
                color: #000;
                margin: 0px;
                padding: 0px;
                
            }
            .store-info p.tax-heading {
                font-size: 18px;
                line-height: 21px;
                font-weight: bold;
                margin-bottom: 10px;
            }
            .store-info .tax-heading {
                font-size: 18px;
                line-height: 21px;
                font-weight: bold;
                margin-bottom: 7px;
            }
            .store-info p.company-name {
                font-size: 11px;
                line-height: 13px;
                font-weight: bold;
                margin-bottom: 4px;
            }
            .store-info p.company-address {
                font-size: 10px;
                line-height: 10px;
                margin-bottom: 3px;
            }
            .store-info p.tax-id-info {
                font-size: 10px;
                line-height: 10px;
            }
            .customer-info-container {
                margin-top: 5px;
            }
            .customer-info-container.fs11 {
                font-size: 11px;
            }
            .customer-info-container .second-row {
                margin-top: 10px;
            }
            .customer-info-container .customer-info-section {
                width: 710px;
            }
            .customer-info-container .customer-info-section .first-container {
                width: 250px;
            }
            .customer-info-container .customer-info-section  label{
                font-weight: bold;
                line-height: 11px;
                color: #000;
                text-transform: uppercase;
                margin-bottom: 5px;
                width: 100%;
            }
            .customer-info-container .customer-info-section .first-container .customer-name {
                font-weight: bold;
                line-height: 11px;
                color: #000;
                width:100%;
                margin-bottom: 3px;
            }
            .customer-info-container .customer-info-section .first-container .customer-address {
                font-weight: normal;
                line-height: 10px;
                color: #000; 
                width: 250px;
            }
            .customer-info-container .customer-info-section .first-container .customer-address1 {
                font-weight: normal;
                line-height: 10px;
                color: #000; 
                margin-bottom: 3px;
            }
            .customer-info-container .customer-info-section .second-container {
                width: 250px;
            }
            .customer-info-container .customer-info-section .second-container label {
                height: 11px;
            }
            .customer-info-container .customer-info-section .second-container p {
                font-weight: normal;
                line-height: 11px;
                color: #000;
                width: 100%;
            }
            .customer-info-container .customer-info-section .second-container p .width100 {
                width: 100%;
            }
            .customer-other-info {
                margin-bottom: 3px;
                line-height: 10px;
                font-weight: normal;
            }
            .customer-info-container .customer-info-section .second-container p  .customer-other-info .email-container {
                width: 90px;
            }
            .M0 {
                margin: 0px !important;
            }
            .bold {
                font-weight: bold;
            }
            .customer-info-container{
                width: 710px;
                margin-bottom: 0px;
            }
            .PL2 {
                padding-right: 2px;
            }
            .invoice-info p {
                line-height: 10px;
                width: 200px !important;
            }
            .invoice-info {
                width: 200px;
            }
            .second-container.ship-to {
                position: absolute;
                left: 300px;
            }
            .ML50 {
                margin-left: 50px !important;
            }
            .customer-info-section.second-row .ship-to-section label {
                width: 51px;
            }
            .customer-info-section.second-row .ship-to-section div {
                width: 185px;
                float: left;
            }
            .contact-label {
                font-weight: bold;
            }
            .contact-name {
                padding-left: 10px;
            }
            .barcode-po {
                margin-top: 1px;
            }
            .MB3 {
                margin-bottom: 3px !important;
            }
            .deal-section {
                width:700px;
                border-bottom: 1px solid black;
                margin-top:20px; 
                float: left;
            }
            .service-section .so-header {
                width:700px;
                border-bottom: 1px solid black;
                margin-top: 25px;
                float: left;
            }
            .service-section .unit {
                width:700px; 
                float: left;
            }
            .service-section .ccc-header, .service-section .cause-body, .service-section .correction-body {
                float:left; 
                width:700px;
                font-size: 14px;
                font-family: Arial Unicode MS,sans-serif;
                margin-bottom:8px;
            }
            .service-section table {
                float:left; 
                width:700px; 
                margin-top:10px;
            }

            .co-deposit-content {
                font-size:14px;
                margin-top: 40px;
            }
            .merchandise-section {
                width:800px;
                padding-top:25px;
            }
            .merchandise-header, .so-header, .deal-header {
                font-weight: bold;
                width:710px !important;
                border-bottom: 1px solid black;
                margin-top:25px;
            }
            .merchandise-label, .so-label, .deal-label {
                width: 300px;
            }
            .sold-by-name {
                display: inline-block;
                width: 250px;
                text-transform: none;
            }
            .merchandise-total, .so-total, .deal-total {
                width: 155px;
                text-align: right;
            }
            .merchandise-table {
                width:710px;
                margin-top:10px;
            }
            .deposit-section {
                width:360px;
                padding-top:6px;
                border: 1px Solid black;
                float:left;
                margin-top: 40px;
            }
            .deposit-header {
                width:360px;
                font-size:14px;
                float:left;
                font-weight:bold;
                text-transform: uppercase;
                border-bottom: 1px solid black;
                margin-bottom:5px;
            }
            .deposit-table {
                width:360px;
            }
            .deposit-row .first-column {
                width:150px;
                padding-left:8px;
            }
            .deposit-row .second-column {
                width:60px;
            }
            .deposit-row .third-column {
                width:70px;
                text-align:right;
                padding-right:8px;
            }
        </style>
    </head>
    <body >
        <div style="{!if(printCODepositRecord.StoreInfo.LogoURL != null, 'height:130px', 'height:70px')};width:800px;margin-bottom:30px;">
            <c:StandardDocumentHeader headerText="CUSTOMER ORDER"
                shipToLabel="SHIP TO"
                orderLabel="ORDER#"/>
        </div>
        <div class="co-deposit-content">
        <div class="merchandise-section">
            <apex:outputPanel rendered="{!printCODepositRecord.MerchandiseCOKHRecord.MerchandiseCOKHList.size > 0}">
                <div class="merchandise-header c-pull-left">
                    <span class="c-pull-left M0 merchandise-label">MERCHANDISE</span>
                    <apex:outputPanel rendered="{!printCODepositRecord.MerchandiseRecord.Salespersons != null}" layout="none">
                        <apex:outputText styleClass="M0 sold-by-name" value="Sold by: {!printCODepositRecord.MerchandiseRecord.Salespersons}"/>
                    </apex:outputPanel>
                    <apex:outputText value="{0, number, currency}" styleClass="c-pull-right M0 merchandise-total">
                        <apex:param value="{!printCODepositRecord.MerchandiseCOKHRecord.Total}"/>
                    </apex:outputText>
                </div>
                <table class="c-pull-left M0 merchandise-table">
                    <thead>
                        <tr>
                            <th style="text-align:left;padding-left:8px;width: 80px;">{!$Label.Label_Type}</th>
                            <th style="text-align:left;padding-left:8px;width: 270px;">Item/Description</th>
                            <th style="text-align:left;padding-left:8px;width: 80px;">{!$Label.Label_Status}</th>
                            <th style="text-align:left;padding-left:8px;width: 50px;">Qty</th>
                            <th style="text-align:left;padding-left:8px;width: 20px;"></th>
                            <th style="text-align:left;padding-left:8px;width: 70px;">{!$Label.Label_Price}</th>
                            <th style="width: 80px;padding-left:8px;text-align:left;">{!$Label.Label_Total}</th>
                         </tr>
                    </thead>
                    
                    <tbody>
                        <apex:repeat value="{!printCODepositRecord.MerchandiseCOKHRecord.MerchandiseCOKHList}" var="COKHItem">
                            <apex:outputPanel rendered="{!COKHItem.id != null}" layout="none">
                                <tr>
                                        <td style= "padding:8px;width: 80px;">Kit</td>
                                        <td style="width:270px;padding-left: 8px;">{!COKHItem.ItemDescription}</td>
                                        <td style="width:80px;"></td>
                                        <td style="width:50px;"></td>
                                        <td style="width:20px;"></td>
                                        <td style="width:70px;"></td>
                                        <td style="width:80px;padding-left: 8px;text-align: left;">
                                            <apex:outputText value="{0, number, currency}" >
                                               <apex:param value="{!COKHItem.Price}"/>
                                            </apex:outputText>
                                        </td>
                                        
                                    </tr>
                            </apex:outputPanel>
                            
                            <apex:repeat value="{!COKHItem.COLIList}" var="COlitem" rendered="{!((COKHItem.Id == null) || (COKHItem.Id != null && !isHideKitDetailsOnDocuments))}">
                                <tr >
                                                    <!-- ng-class-even="'C2'" -->                                               
                                                    <td style="padding: 8px;width:80px; {!if(COKHItem.Id != null, 'padding-left:16px', '')};">
                                                        <apex:outputPanel rendered="{!COlitem.IsPart}">Part</apex:outputPanel>
                                                        <apex:outputPanel rendered="{!COlitem.IsFee}">Fee</apex:outputPanel>
                                                    </td> 
                                                    <td  style="width:270px;padding-left: 8px;">
                                                    	<apex:outputText rendered="{!COlitem.IsPart && isHidePartNumbersOnDocuments}" value="{!COlitem.ItemDescription}"></apex:outputText>
                                                    	<apex:outputText rendered="{!!(COlitem.IsPart && isHidePartNumbersOnDocuments)}" value="{!COlitem.Item}"></apex:outputText>
                                                    </td>
                                                     <td style="width:80px;padding-left: 8px;">
                                                        <apex:outputPanel rendered="{!(COlitem.IsPart && !COlitem.NonInventoryPart) || !COlitem.IsPart}">
                                                                <apex:outputText value="{!COlitem.Status}" >
                                                            </apex:outputText>
                                                            </apex:outputPanel>
                                                            <apex:outputPanel rendered="{!COlitem.IsPart && COlitem.NonInventoryPart}">
                                                                <apex:outputText value="{!IF(COlitem.Status == 'Invoiced' , 'Invoiced' , 'In Stock')}" >
                                                            </apex:outputText>
                                                            </apex:outputPanel>
                                                        </td>
                                                    <td style="padding-left: 8px;width:50px;">
                                                        <apex:outputText value="{0}">
                                                            <apex:param value="{!COlitem.Qty}" />
                                                        </apex:outputText>
                                                    </td>
                                                    <td style="width:20px;" > @</td>
                                                    <td style="padding-left: 8px;width:70px; ">
                                                        <apex:outputText value="{0, number, currency}" >
                                                            <apex:param value="{!COlitem.Price}"/>
                                                        </apex:outputText>
                                                    </td>
                                                    <td style="padding-left: 8px;text-align: left;width:80px;">
                                                        <apex:outputPanel rendered="{!COKHItem.Id == null}" layout="none">
                                                            <apex:outputText value="{0, number, currency}" rendered="{!COlitem.SubTotal == null}">
                                                                <apex:param value="{!COlitem.Qty * COlitem.Price}"/>
                                                            </apex:outputText>
                                                            <apex:outputText value="{0, number, currency}" rendered="{!COlitem.SubTotal != null}">
                                                                <apex:param value="{!COlitem.SubTotal}"/>
                                                            </apex:outputText>
                                                        </apex:outputPanel>
                                                    </td>
                                            </tr>
                            </apex:repeat>
                      </apex:repeat>
                    </tbody>
                    
                </table>
            </apex:outputPanel>
            
            <apex:outputPanel >
                <apex:repeat value="{!printCODepositRecord.SOHeadersList}" var="soHeaderVar">
                    <div class="service-section">
                        <div class="so-header c-pull-left">
                            <span class="c-pull-left M0 so-label">{!soHeaderVar.SoHeaderName}</span>
                            <apex:outputPanel rendered="{!soHeaderVar.Salespersons != null}" layout="none">
                                <apex:outputText styleClass="M0 sold-by-name" value="Sold by: {!soHeaderVar.Salespersons}"/>
                            </apex:outputPanel>
                            <apex:outputText value="{0, number, currency}" styleClass="c-pull-right M0 so-total">
                                <apex:param value="{!soHeaderVar.Total}"/>
                            </apex:outputText>
                        </div>
                        <div class="unit">
                            <apex:outputPanel rendered="{!soHeaderVar.Unit != null}">
                                <div style="width:90px;margin-top: 10px;display: inline-block;">
                                    <label style="font-weight: bold;padding-left:8px;">UNIT:</label>
                                </div>
                                <div style="width:579px;display: inline-block;padding-left:20px;">
                                    {!soHeaderVar.Unit}
                                </div>
                            </apex:outputPanel>
                        </div>
                        <div class="ccc-header">
                            <apex:outputPanel rendered="{!soHeaderVar.ConcernList.size > 0}">
                                <apex:outputPanel rendered="{!soHeaderVar.ConcernList.size == 1}" layout="none">
                                    <div style="width:72px;display: inline-block;margin-left:8px;margin-top: 10px;">
                                        <label style="font-weight: bold;">{!$Label.Label_Concern}:  </label>
                                    </div>
                                    <div style="width:560px;display: inline-block;padding-left:32px;">
                                        <span>{!soHeaderVar.ConcernList[0]}</span>
                                    </div>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!soHeaderVar.ConcernList.size > 1}" layout="none">
                                    <div style="width:90px;position: absolute;margin-left:18px;margin-top: 10px;">
                                        <label style="font-weight: bold;">{!$Label.Label_Concern}:  </label>
                                    </div>
                                    <div style="width:599px;position: relative;margin-left: 48px;">
                                        <ul style="padding-left:40px;">
                                            <apex:repeat value="{!soHeaderVar.ConcernList}" var="concern">
                                                <li>{!concern}</li>
                                            </apex:repeat>
                                        </ul>
                                    </div>
                                </apex:outputPanel>
                            </apex:outputPanel>
                        </div>
                        <div class="cause-body">
                            <apex:outputPanel rendered="{!soHeaderVar.CauseList.size > 0}">
                                <apex:outputPanel rendered="{!soHeaderVar.CauseList.size == 1}" layout="none">
                                    <div style="width:72px;display: inline-block;margin-left:8px;margin-top: 10px;">
                                        <label style="font-weight: bold;">{!$Label.Label_Cause}:  </label>
                                    </div>
                                    <div style="width:560px;display: inline-block;padding-left:32px;">
                                        <span>{!soHeaderVar.CauseList[0]}</span>
                                    </div>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!soHeaderVar.CauseList.size > 1}" layout="none">
                                    <div style="width:90px;position: absolute;margin-left:18px;margin-top: 10px;">
                                        <label style="font-weight: bold;">{!$Label.Label_Cause}:  </label>
                                    </div>
                                    <div style="width:599px;position: relative;margin-left: 48px;">
                                        <ul style="padding-left:40px;">
                                            <apex:repeat value="{!soHeaderVar.CauseList}" var="cause">
                                                <li>{!cause}</li>
                                            </apex:repeat>
                                        </ul>
                                    </div>
                                </apex:outputPanel>
                            </apex:outputPanel>
                        </div>
                        <div class="correction-body">
                            <apex:outputPanel rendered="{!soHeaderVar.CorrectionList.size > 0}">
                                <apex:outputPanel rendered="{!soHeaderVar.CorrectionList.size == 1}" layout="none">
                                    <div style="width:72px;display: inline-block;margin-left:8px;margin-top: 10px;">
                                        <label style="font-weight: bold;">{!$Label.Label_Correction}:  </label>
                                    </div>
                                    <div style="width:560px;display: inline-block;padding-left:32px;">
                                        <span>{!soHeaderVar.CorrectionList[0]}</span>
                                    </div>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!soHeaderVar.CorrectionList.size > 1}" layout="none">
                                    <div style="width:90px;position: absolute;margin-left:18px;margin-top: 10px;">
                                        <label style="font-weight: bold;">{!$Label.Label_Correction}:  </label>
                                    </div>
                                    <div style="width:599px;position: relative;margin-left: 48px;">
                                        <ul style="padding-left:40px;">
                                            <apex:repeat value="{!soHeaderVar.CorrectionList}" var="correction">
                                                <li>{!correction}</li>
                                            </apex:repeat>
                                        </ul>
                                    </div>
                                </apex:outputPanel>
                            </apex:outputPanel>
                        </div>
                        <table>
                            <thead>
                                <tr>
                                    <th style="text-align:left;padding-left:8px;width: 80px;">{!$Label.Label_Type}</th>
                                    <th style="text-align:left;padding-left:16px;width: 270px;">Item/Description</th>
                                    <th style="text-align:left;padding-left:8px;width: 80px;">{!$Label.Label_Status}</th>
                                    <th style="text-align:left;padding-left:8px;width: 50px;">Qty</th>
                                    <th style="text-align:left;padding-left:8px;width: 20px;"></th>
                                    <th style="text-align:left;padding-left:8px;width: 70px;">{!$Label.Label_Price}</th>
                                    <th style="text-align:left;padding-left:8px;width: 80px;">{!$Label.Label_Total}</th>
                                </tr>
                            </thead>
                        
                            <tbody>
                                <apex:repeat value="{!soHeaderVar.SOGridItems}" var="soGrid">
                                    <apex:outputPanel rendered="{!soGrid.SOKH.id != null}" layout="none">
                                        <tr>
                                            <td style= "padding:8px;width: 80px;">Kit</td>
                                            <td style="width: 270px;padding-left:16px;">{!soGrid.SOKH.ItemDescription}</td>
                                            <td style="width: 80px;"></td>
                                            <td style="width: 50px;"></td>
                                            <td style="width: 20px;"></td>
                                            <td style="width: 70px;"></td>
                                            <td style="text-align: left ;padding-left:8px;width: 80px;">
                                                <apex:outputText value="{0, number, currency}" >
                                                    <apex:param value="{!soGrid.SOKH.Price}"/>
                                                </apex:outputText>
                                            </td>
                                        </tr>
                                    </apex:outputPanel>
                                    
                                    <apex:repeat value="{!soGrid.SOKH.SOLIList}" var="SoLi" rendered="{!((soGrid.SOKH.Id == null) || (soGrid.SOKH.Id != null && !isHideKitDetailsOnDocuments))}">
                                        <tr >                                              
                                            <td style="padding: 8px;width: 80px; {!if(soGrid.SOKH.Id != null, 'padding-left:16px', '')};">
                                                <apex:outputPanel rendered="{!SoLi.IsPart}">Part</apex:outputPanel>
                                                <apex:outputPanel rendered="{!SoLi.IsFee}">Fee</apex:outputPanel>
                                                <apex:outputPanel rendered="{!SoLi.IsLabour}">Labor</apex:outputPanel>
                                                <apex:outputPanel rendered="{!SoLi.IsSublet}">Sublet</apex:outputPanel>
                                            </td>
                                            <td  style="width:270px;padding-left:16px;">
                                                <apex:outputText rendered="{!SoLi.IsPart && isHidePartNumbersOnDocuments}" value="{!SoLi.ItemDescription}"></apex:outputText>
                                                <apex:outputText rendered="{!!(SoLi.IsPart && isHidePartNumbersOnDocuments)}" value="{!SoLi.ItemDesc}"></apex:outputText>
                                            </td>
                                            <td style="width:80px;padding-left:8px;">
                                                <apex:outputPanel rendered="{!SoLi.IsPart && !SoLi.IsNonInventoryPart}">
                                                    <apex:outputText value="{!SoLi.Status}" >
                                                    </apex:outputText>  
                                                </apex:outputPanel>
                                                <apex:outputPanel rendered="{!SoLi.IsPart && SoLi.IsNonInventoryPart}">
                                                    <apex:outputText value="{!IF(SoLi.Status == 'Invoiced' , 'Invoiced' , 'In Stock')}" ></apex:outputText>
                                                </apex:outputPanel>
                                                <apex:outputPanel rendered="{!SoLi.IsFee || SoLi.IsLabour || SoLi.IsSublet}">
                                                    <apex:outputText value="{!IF(soHeaderVar.SoHeaderInvoiceStatus == 'Invoiced' , 'Invoiced' , 'Ready')}" >
                                                    </apex:outputText>
                                                </apex:outputPanel>
                                            </td>
                                            <td style="text-align: left;width:50px;padding-left:8px;">
                                                <apex:outputText value="{0}" rendered="{!!printCODepositRecord.IsHideHoursRateOnServiceDocuments || !SoLi.IsLabour}">
                                                    <apex:param value="{!SoLi.QtyNeeded}" />
                                                </apex:outputText>
                                            </td>
                                            <td style="width:20px;">
                                                <apex:outputText value="@"
                                                rendered="{!!printCODepositRecord.IsHideHoursRateOnServiceDocuments || !SoLi.IsLabour}"/>
                                            </td>              
                                            <td style="text-align:left;width:70px;padding-left:8px; ">
                                                <apex:outputPanel rendered="{!!printCODepositRecord.IsHideHoursRateOnServiceDocuments || !SoLi.IsLabour}">
                                                    <apex:outputText value="{0, number, currency}">
                                                        <apex:param value="{!SoLi.KitPrice}"/>
                                                    </apex:outputText>
                                                </apex:outputPanel>
                                                <apex:outputPanel rendered="{!(!(soGrid.SOKH.Id == null) && SoLi.IsLabour && printCODepositRecord.IsHideHoursRateOnServiceDocuments )}">
                                                    <apex:outputText value="{0, number, currency}">
                                                        <apex:param value="{!SoLi.QtyNeeded * SoLi.KitPrice}"/>
                                                    </apex:outputText>
                                                </apex:outputPanel>
                                            </td>
                                            <td style="text-align:left;width:80px;padding-left:8px;">
                                                <apex:outputPanel rendered="{!soGrid.SOKH.Id == null}" layout="none">
                                                    <apex:outputText value="{0, number, currency}">
                                                        <apex:param value="{!SoLi.QtyNeeded * SoLi.KitPrice}"/>
                                                    </apex:outputText>
                                                </apex:outputPanel>
                                            </td>
                                        </tr>
                                    </apex:repeat>
                                </apex:repeat>
                            </tbody>
                        </table>
                    </div>
                </apex:repeat>
            </apex:outputPanel>        
        
        
        
        
        
        
        
        
        
        <apex:outputPanel rendered="{!printCODepositRecord.DealInfo.UnitList.size > 0 || printCODepositRecord.DealInfo.TradeInList.size>0}">
                <div class="deal-header c-pull-left">
                    <span class="c-pull-left M0 deal-label">DEAL</span>
                    <apex:outputPanel rendered="{!printCODepositRecord.DealInfo.Salespersons != null}" layout="none">
                        <apex:outputText styleClass="M0 sold-by-name" value="Sold by: {!printCODepositRecord.DealInfo.Salespersons}"/>
                    </apex:outputPanel>
                    <apex:outputText value="{0, number, currency}" styleClass="c-pull-right M0 deal-total">
                        <apex:param value="{!printCODepositRecord.DealInfo.DealTotal}"/>
                    </apex:outputText>
                </div>
