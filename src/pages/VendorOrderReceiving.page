<apex:page standardStylesheets="false" showChat="false" showHeader="false" sidebar="false" docType="html-5.0"  controller="GlobalController"
	cache="true" expires="{!cacheExpiresIn}">
        <c:VendorInfo />
        <c:COPopup />
        <c:PartPopUpOnVendorOrder />
        <div>
            <route-loading-indicator></route-loading-indicator>
            <div ng-cloak="ng-cloak"> <!-- ng-if="!isRouteLoading"  -->
            <!-- START: Page Content -->
            <div class="content">

                <!-- START: Side panel start -->
                <div class="col-lg-2 col-md-2 sidePanel">
                    <div class="clearfix"></div>
                    <div class="orderNumber row">
                        <span>{{ 'Label_Order' | translate }} #: 
                            <span>{{VORModel.VOR_Header.VORName}}</span>
                        </span>
                    </div>
                    <div class="sidepaneluserinfo row" style="padding-bottom: 5px;">
                        <div class="userNameblock"> <!-- style="margin-top: 0px;" -->
                            <p> 
                                <a ng-mouseover="VORModel.showVendorInfoOverlay($event, VORModel.VOR_Header.VendorId);" 
                                    ng-mouseleave="VORModel.hideVendorInfoOverlay();" id="CO_workflow_sidepanel_cust_name_link">
                                    {{VORModel.VOR_Header.VendorName}}</a>
                            </p>
                            <div class="vr-status">
                                <span class="userStatus">
                                    {{VORModel.VOR_Header.Status}}
                                </span>
                            <div class="MT5" ng-if="VORModel.VOR_Header.Status == 'Stocked' && GroupOnlyPermissions['Vendor receiving']['create/modify']">
                                <button id="VR_workflow_sidepanel_info_div1_button1"
                                    class="button primary active_btn small"
                                    ng-click="VORModel.openReopenVRModel()">Reopen VR
                                </button>
                            </div>
                        </div>
                    </div>
                    </div>
                    <div class="clearfix"></div>
                    <!-- start: leftPanelLinks  -->
                    <div class="leftPanelLinks" style="height: 375px">
                        <a class="row T10 mainSectionLink"
                            ng-class="{'active' : VORModel.activeSidepanelink =='#InfoSection'}"
                            href="#VOR_Info_Block"
                            ng-click="VORModel.sidepanelLink($event, 'Info')">
                            <span class="alphabetLetter">{{VORModel.LeftSideHeadingLables.Info}}</span>{{ 'Page_Section_Info' | translate }}
                        </a> 
                        <a class="row T10 subSectionLink" href="#VOR_Info_Block"
                            ng-click="VORModel.sidepanelLink($event, 'Info')">
                            <span class="alphabetLetter"></span>{{ 'Label_General' | translate }}
                        </a> 
                        <a class="row T10 subSectionLink"
                            href="#VOR_Vendor_Order_Receiving_Block"
                            ng-click="VORModel.sidepanelLink($event, 'Info')"
                            ng-show="VORModel.vendorOrdersList.length != 0 && VORModel.VOR_Header.VendorName != null && VORModel.vendorOrdersList.length != 0 && VORModel.VOR_Header.Status != 'Stocked' && VORModel.VOR_Header.Status != 'Invoiced'">
                            <span class="alphabetLetter"></span>{{ 'Vendor_Receiving_For_Vendor_Order' | translate }}
                        </a>
                        <a class="row T10 mainSectionLink" 
                            ng-if="VORModel.VOR_Header.VendorName != null && (VORModel.VORLineItemGroupList.length != 0 ||  VORModel.outstandingVOGList.length != 0 || VORModel.actualTotalOutstandingItems != 0 || VORModel.actualTotalGroupItems != 0)"
                            ng-class="{'active' : VORModel.activeSidepanelink =='#ReceivedSection'}"
                            href="#VOR_Vendor_Received_Block"
                            ng-click="VORModel.sidepanelLink($event, 'Received')">
                            <span class="alphabetLetter">{{VORModel.LeftSideHeadingLables.Received}}</span>{{ 'Label_Received' | translate }}
                        </a>
                        <a class="row T10 subSectionLink" href="#VOR_Items_Block"
                            ng-if="VORModel.VOR_Header.VendorName != null && VORModel.actualTotalGroupItems != 0"
                            ng-click="VORModel.sidepanelLink($event, 'Received')">
                            <span class="alphabetLetter"></span>{{ 'Items' | translate }}
                        </a>
                        <a class="row T10 subSectionLink " style="margin-left: 18px"
                            href="#VOR_Outstanding_Items_Block"
                            ng-click="VORModel.sidepanelLink($event, 'Received')"
                            ng-show="VORModel.VOR_Header.VendorId != null && VORModel.actualTotalOutstandingItems != 0 && VORModel.VOR_Header.Status == 'In Progress' 
                               && VORModel.VOR_Header.VendorName != null">
                            {{ 'Outstanding_items' | translate }} <!-- for {{VORModel.SelectedVendorOrdersList[0]}} --> 
                        </a>  
                        <a class="row T10 mainSectionLink"
                            ng-class="{'active' : VORModel.activeSidepanelink =='#FinalizeSection'}"
                            href="#VOR_Finalize_Block"
                            ng-click="VORModel.sidepanelLink($event, 'Finalize')"
                            ng-show="VORModel.VOR_Header.VendorId != null &&  VORModel.VOR_Header.Status == 'In Progress'
                                   && VORModel.VOR_Header.VendorName != null">
                            <span class="alphabetLetter">{{VORModel.LeftSideHeadingLables.Finalize}}</span>{{ 'Finalize' | translate }}
                        </a>
                        <a class="row T10 mainSectionLink"
                            ng-class="{'active' : VORModel.activeSidepanelink =='#InvoiceHistorySection'}"
                            href="#VOR_Vendor_Invoicing_Block"
                            ng-click="VORModel.sidepanelLink($event, 'InvoiceHistory')"
                            ng-show="VORModel.VOR_Header.VendorId != null && (VORModel.VOR_Header.Status == 'Stocked' || VORModel.VOR_Header.Status == 'Invoiced') 
                                  && VORModel.VOR_Header.VendorName != null
                                  && VORModel.VIHistoryList.length != 0">
                            <span class="alphabetLetter">{{VORModel.LeftSideHeadingLables.Invoice_History}}</span>{{ 'Label_Invoice_History' | translate }}
                        </a>
                    </div>
                    <!-- end: leftPanelLinks  -->
                    
                    <!-- start: sideBarTotals  -->
                    <div class="sideBarTotals">
                       <div class="totalDue C1">
                            <a class="T2">{{ 'Total_Items' | translate }}:</a>
                            <a class="T2 text-right"> {{VORModel.VOR_Header.TotalLineItem}}</a>
                        </div>
                        <div class="totalDue C1">
                            <a class="T2">{{ 'Total_Pieces' | translate }}:</a>
                            <a class="T2 text-right"> {{VORModel.VOR_Header.TotalReceivedQty | number : 0}}</a>
                        </div>
                        <div class="totalDue C1">
                            <a class="T2">{{ 'Total_Cost_Label' | translate }}:</a>
                            <a class="T2 text-right"> {{VORModel.GroupTotalCost | currency}}<bp-flag ng-if="VORModel.IsForeignExchangeCostingEnabled && VORModel.VOR_Header.IsForeignVendor" currencyCode = "{{VORModel.VOR_Header.FXCurrencyDetails.CurrencyCode}}"></bp-flag></a>
                        </div>
                    </div>
                    <!-- end: sideBarTotals  -->
                </div>
                <!-- END: Left Side Bar -->

                <!-- START: Center Panel -->
                <div class="col-lg-10 col-md-10 mainSection">
                    <div class=" sectionHeader row fixedRow">
                    
                        <!-- Start: fixed header Info Section -->
                        <div class="col-lg-6 col-md-6"
                            ng-show="VORModel.activeSidepanelink =='#InfoSection'">
                            <div class="fixedHeaderText">
                                <span class="headerlabel">
                                    <i class="fa fa-angle-right" ng-class="{'defaultCursor' : !GroupOnlyPermissions['Vendor receiving']['view']}"
                                        style="width: 10px; padding: 0 3px;"
                                        ng-show="!VORModel.displaySections.Info"
                                        ng-Click="VORModel.showSections('Info')"> 
                                    </i> 
                                    <i class="fa fa-angle-down" style="width: 10px;" ng-class="{'defaultCursor' : !GroupOnlyPermissions['Vendor receiving']['view']}"
                                        ng-show="VORModel.displaySections.Info"
                                        ng-Click="VORModel.showSections('Info')">
                                    </i>
                                    <span class="divider">|</span>
                                    <span class="headdingFirstText">{{ 'A' | translate }}</span>
                                    <span class="T2">{{ 'Page_Section_Info' | translate }}</span>
                                </span>
                                <span class="ActionIcons pull-left"
                                    ng-show="VORModel.showDeleteReceivingLink()">
                                    <div class="dropdown selectDropdown checkboxDropdown">
                                        <button class="btn dropdown-toggle" type="button" data-toggle="dropdown" ng-disabled = "!GroupOnlyPermissions['Vendor receiving']['create/modify']">{{ 'Label_Actions' | translate }}
                                            <span class="caret"></span>
                                        </button>
                                        <ul class="dropdown-menu">
                                            <li>
                                                <a ng-click="VORModel.vendorRecievingDelete()">{{ 'Delete_receiving' | translate }}</a>
                                            </li>
                                        </ul>
                                    </div>
                                </span> 
                            </div>
                        </div>
                        <!-- End: fixed header Info Section -->
                        
                        <!-- Start: condition when VendorName null -->
                        <div ng-if="VORModel.VOR_Header.VendorName != null">
                        
                            <!-- Start: fixed header Received Section-->
                            <div class="col-lg-4 col-md-4"
                                ng-show="VORModel.activeSidepanelink =='#ReceivedSection' && (VORModel.VORLineItemGroupList.length != 0 ||  VORModel.outstandingVOGList.length != 0 || VORModel.actualTotalOutstandingItems != 0 || VORModel.actualTotalGroupItems != 0)">
                                <div class="fixedHeaderText">
                                    <span class="headerlabel"> 
                                        <i class="fa fa-angle-right"
                                        ng-class="{'defaultCursor' : !GroupOnlyPermissions['Vendor receiving']['view']}" style="width: 10px; padding: 0 3px;"
                                            ng-show="!VORModel.displaySections.Received"
                                            ng-Click="VORModel.showSections('Received')"> 
                                        </i> 
                                        <i class="fa fa-angle-down" style="width: 10px;" ng-class="{'defaultCursor' : !GroupOnlyPermissions['Vendor receiving']['view']}"
                                            ng-show="VORModel.displaySections.Received"
                                            ng-Click="VORModel.showSections('Received')"> 
                                        </i> 
                                        <span class="divider">|</span> 
                                        <span class="headdingFirstText">{{VORModel.LeftSideHeadingLables.Received}}</span>
                                        
                                        <span class="T2">{{ 'Label_Received' | translate }}</span>
                                    </span>
                                    <span class="ActionIcons pull-left"
                                        > 
                                        <div class="dropdown selectDropdown checkboxDropdown">
                                            <button class="btn dropdown-toggle" type="button" ng-disabled = "VORModel.VORLineItemGroupList.length == 0" data-toggle="dropdown">{{ 'Label_Actions' | translate }}
                                                <span class="caret"></span>
                                            </button>
                                            <ul class="dropdown-menu">
                                                <li>
                                                    <a data-toggle="modal" ng-click = "VORModel.getLabelPrintingData(VORModel.VOR_Header.VORId)">{{ 'Print_barcode_labels' | translate }}</a>
                                                </li>
                                            </ul>
                                        </div>
                                </span>
                                    <span class="ActionIcons service_order_print_icon pull-left " 
                                     ng-class="{'defaultCursor' : !GroupOnlyPermissions['Vendor receiving']['view']}"
                                    ng-if="VORModel.VORLineItemGroupList.length != 0">
                                        <i class="fa fa-print"
                                            ng-click="VORModel.PrintVendorReceiving()">
                                        </i>
                                    </span>
                                    <span class="ActionIcons pull-left" ng-if="VORModel.outstandingVOGList.length > 0">
                                        <div class="VR_OrderSection">
                                            <button class="btn btn-default addSelectedBtnDisabled text-uppercase" 
                                                ng-click="VORModel.disableAddSelectedButton = true; VORModel.addSelectedItemsFromOutstandingToItemSection()"
                                                ng-class="{'addSelectedBtnActive' : VORModel.enableSelectedBtnInReceivedSection()}"
                                                ng-disabled="(VORModel.disableAddSelectedButton || !VORModel.enableSelectedBtnInReceivedSection())">Add Selected
                                            </button>
                                        </div>
                                    </span>
                                </div>
                            </div>
                            <!-- End: fixed header Received Section-->
                            
                            <!-- Start: fixed header Finalize Section-->
                            <div class="col-lg-4 col-md-4" ng-show="VORModel.activeSidepanelink =='#FinalizeSection'">
                                <div class="fixedHeaderText">
                                    <span class="headerlabel"> 
                                        <i class="fa fa-angle-right" ng-class="{'defaultCursor' : !GroupOnlyPermissions['Vendor receiving']['view']}" style="width: 10px; padding: 0 3px;"
                                            ng-show="!VORModel.displaySections.Finalize"
                                            ng-Click="VORModel.showSections('Finalize')"> 
                                        </i>
                                        <i class="fa fa-angle-down" style="width: 10px;" ng-class="{'defaultCursor' : !GroupOnlyPermissions['Vendor receiving']['view']}"
                                            ng-show="VORModel.displaySections.Finalize"
                                            ng-Click="VORModel.showSections('Finalize')">
                                        </i>
                                        <span class="divider">|</span>
                                        <span class="headdingFirstText">{{VORModel.LeftSideHeadingLables.Finalize}}</span>
                                        <span class="T2">{{ 'Finalize' | translate }}</span>
                                    </span>
                                    <span class="ActionIcons pull-left ">
                                        <div class="dropdown selectDropdown checkboxDropdown">
                                        </div>
                                    </span>
                                </div>
                            </div>
                            <!-- End: fixed header Finalize Section-->
                            
                            <!-- Start: fixed header Invoice History Section-->
                            <div class="col-lg-4 col-md-4"
                                ng-show="VORModel.activeSidepanelink =='#InvoiceHistorySection' && VORModel.VIHistoryList.length != 0">
                                <div class="fixedHeaderText">
                                    <span class="headerlabel">
                                        <i class="fa fa-angle-right" ng-class="{'defaultCursor' : !GroupOnlyPermissions['Vendor receiving']['view']}" style="width: 10px; padding: 0 3px;"
                                            ng-show="!VORModel.displaySections.InvoiceHistory"
                                            ng-Click="VORModel.showSections('InvoiceHistory')">
                                        </i>
                                        <i class="fa fa-angle-down" style="width: 10px;" ng-class="{'defaultCursor' : !GroupOnlyPermissions['Vendor receiving']['view']}"
                                            ng-show="VORModel.displaySections.InvoiceHistory"
                                            ng-Click="VORModel.showSections('InvoiceHistory')">
                                        </i>
                                        <span class="divider">|</span> 
                                        <span class="headdingFirstText">{{VORModel.LeftSideHeadingLables.Invoice_History}}</span>
                                        <span class="T2">{{ 'Label_Invoice_History' | translate }}</span>
                                    </span>
                                    <span class="ActionIcons pull-left ">
                                        <div class="dropdown selectDropdown checkboxDropdown">
                                        </div>
                                    </span>
                                </div>
                            </div>
                            <!-- End: fixed header Invoice History Section-->
                            
                        </div>
                        <!-- End: condition when VendorName null -->
                        
                        <!-- Start: fixed header Content -->
                        <div class="col-lg-6 col-md-6 fixedContent">
                            <i class="fa fa-refresh fa-2x fa-fw pull-left"
                                ng-class="{'fa-spin' : VORModel.isrefresh}"
                                ng-click="VORModel.loadVendorRefresh();"> <!-- pull-left Added by richa #143 03/10/2016 -->
                            </i>
                            <div class="col-lg-10 col-md-9 searchToAdd pull-left"> <!-- pull-left Added by richa, searchToAddMR Removed by richa #143 03/10/2016 -->
                                <div class="input-group" ng-class="{'activeSearchDiv' : VORModel.searchDivActive}">
                                    <div class="input-group-btn">
                                        <button type="button" class="btn C5 LabelBtn">
                                            <span class="T5" style="color: #939CA9;">{{ 'Label_To' | translate }}:</span>
                                            <span style="color: #FFFFFF;">{{VORModel.selectedItem}}</span>
                                        </button>
                                        <button type="button" class="btn C5 dropdown-toggle caretBtn"
                                            data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                            <span class="caret"></span> <span class="sr-only">{{ 'Label_Toggle_Dropdown' | translate }}</span>
                                        </button>
                                        <ul class="dropdown-menu">
                                            <li ng-click="VORModel.UpdateSelectedSection($event,$index)">
                                                <a href="#VOR_Info_Block"
                                                    ng-click="VORModel.dropDownItem($event, 'Info')">{{ 'Page_Section_Info' | translate }}
                                                </a> 
                                                <a
                                                    ng-if="VORModel.VOR_Header.VendorName != null && (VORModel.VORLineItemGroupList.length != 0 ||  VORModel.outstandingVOGList.length != 0 || VORModel.actualTotalOutstandingItems != 0 || VORModel.actualTotalGroupItems != 0)"
                                                    href="#VOR_Vendor_Received_Block"
                                                    ng-click="VORModel.dropDownItem($event, 'Received')">{{ 'Label_Received' | translate }}
                                                </a>
                                                <a href="#VOR_Finalize_Block"
                                                    ng-click="VORModel.dropDownItem($event, 'Finalize')"
                                                    ng-if="VORModel.VOR_Header.VendorId != null && VORModel.VOR_Header.Status == 'In Progress'
                                                           && VORModel.VOR_Header.VendorName != null">{{ 'Finalize' | translate }}
                                                    <!-- && (VORModel.VOR_Header.Status != 'Stocked' && VORModel.VOR_Header.PackagingSlipNumber != undefined && VORModel.VOR_Header.PackagingSlipNumber != '' && VORModel.VOR_Header.PackagingSlipNumber != null) Commented by richa -->
                                                </a>
                                                <a href="#VOR_Vendor_Invoicing_Block"
                                                    ng-click="VORModel.dropDownItem($event, 'InvoiceHistory')"
                                                    ng-if="VORModel.VOR_Header.VendorId != null && (VORModel.VOR_Header.Status == 'Stocked' || VORModel.VOR_Header.Status == 'Invoiced') 
                                                       && VORModel.VOR_Header.VendorName != null 
                                                       && VORModel.VIHistoryList.length != 0">{{ 'Label_Invoice_History' | translate }}
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                    <!-- /btn-group -->
        
                                    <div
                                        class="inner-addon right-addon neVendorOrderAutocomplete-container" ng-class="{'disabledSearchToAddElement' : VORModel.VOR_Header.Status == 'Stocked' || VORModel.VOR_Header.Status == 'Invoiced' }">
                                        <i class="fa fa-search fa-flip-horizontal"></i>
                                        <c:AutoSuggestVendorOrder />
                                    </div>
                                </div>
                                <!-- /input-group -->
                            </div>
        
        
                        </div>
                        <!-- End: fixed header Content -->
            </div>

            <!-- Start: contentWrapper -->
            <div class="contentWrapper">
                <!-- Start: Info section -->
                <div id="VOR_Info_Block" ng-show="VORModel.displaySections.Info">
                    <!-- START: General Section -->
                    <div class="main_section row VOR_Info_Block main_section_margin_top">
                        <div class="row rowGeneral">
                            <div class="content ">
                                <div class="col-lg-12 col-md-12 Pl24 MB48"
                                    id="VOR_General_Block">
                                    <p class="actionRow T2">
                                        <span class="sectionText">{{ 'Label_General' | translate }} </span>
                                        <span class="ActionIcons"> <i class="fa fa-question-circle"
                                            data-toggle="tooltip" title="General Info"></i>
                                        </span>
                                    </p>
                                    <!-- START: Vendor selection section -->
                                    <div class="col-lg-7 col-md-7 Plr12">
                                        <p class="T3 MB24 F12 FC2">{{ 'Vendor_Object_Display_Label' | translate }}</p>
                                        <!-- style="margin-left: 35px;" -->
                                        <div class="col-md-6 Plr0 ">
                                            <div class="col-lg-8 col-md-7 PLU MB12 ">
                                                <div ng-if="VORModel.VOR_Header.VendorName == null">
                                                    <span class="T1_SubHeading"> <a 
                                                        ng-click="VORModel.RelatedList_addAction($event, 'Vendor')"
                                                        id="CO_workflow_sidepanel_add_cust_link">{{ 'Add_vendor' | translate }}</a>
                                                    </span>
                                                </div>
                                                <div ng-if="VORModel.VOR_Header.VendorName!= null">
                                                    <p class="T1 ">
                                                        <a ng-mouseover="VORModel.showVendorInfoOverlay($event, VORModel.VOR_Header.VendorId);" 
                                                            ng-mouseleave="VORModel.hideVendorInfoOverlay();" id="CO_workflow_sidepanel_cust_name_link">
                                                            {{VORModel.VOR_Header.VendorName}}</a>
                                                    </p>
                                                </div>

                                            </div>
                                            
                                            <div class="col-md-12 Plr0 MB24 ">
                                                <div class="generalInfoData">
                                                    <label class="T3">{{ 'Label_Status' | translate }}</label>
                                                </div>
                                                <div class="mycustvalue viStatus">
                                                    <span class="FC10 T3"><!--  style="color: green" -->
                                                        {{VORModel.VOR_Header.Status}}
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-12  Plr0 dates-container order-type-container">
                                            <span class="T3 col-md-12 Plr0 MB12" ng-if="VORModel.VOR_Header">{{ 'Label_Dates' | translate }}</span>
                                            <div class="col-md-10 Plr0 MB6 order-type-container" ng-if = "VORModel.VOR_Header.CreatedDate != '' && VORModel.VOR_Header.CreatedDate != null">
                                                <div class="date-section">
                                                    <span class="col-md-4 T7 Plr0">{{ 'Label_Created' | translate }} {{ 'Label_Date' | translate }}:</span>
                                                    <span class="col-md-7 T7 Plr0">{{VORModel.VOR_Header.CreatedDate}}</span>
                                                </div>
                                                <div class="username-section">
                                                    <span class="col-md-4 T7 Plr0 field-label">By:</span>
                                                    <span class="col-md-7 T7 Plr0">{{VORModel.VOR_Header.CreatedBy}}</span>
                                                </div>
                                            </div> 
                                            <div class="col-md-10 Plr0 MB6 order-type-container" ng-if = "VORModel.VOR_Header.LastModifiedDate != '' && VORModel.VOR_Header.LastModifiedDate != null">
                                                <div class="date-section">
                                                    <span class="col-md-4 T7 Plr0">{{ 'Label_Modified' | translate }}:</span>
                                                    <span class="col-md-7 T7 Plr0">{{VORModel.VOR_Header.LastModifiedDate}}</span>
                                                </div>
                                                <div class="username-section">
                                                    <span class="col-md-4 T7 Plr0 field-label">By:</span>
                                                    <span class="col-md-7 T7 Plr0">{{VORModel.VOR_Header.LastModifiedBy}}</span>
                                                </div>
                                            </div>
                                            <div class="col-md-10 Plr0 MB6 order-type-container" ng-if = "VORModel.VOR_Header.FinalizedDate != '' && VORModel.VOR_Header.FinalizedDate != null">
                                                <span class="col-md-4 T7 Plr0">{{ 'Label_Finalized' | translate }}:</span>
                                                <span class="col-md-7 T7 Plr0">{{VORModel.VOR_Header.FinalizedDate}}</span>
                                            </div> 
                                        </div>
                                        <div class="col-md-6 Plr0 ">
                                            <div class="col-md-12 Plr0 Mrg20" ng-if="GroupOnlyPermissions['Vendor invoicing']['create/modify']">
                                                <div class="c-slider-container receiving-from-an-invoice">
                                                    <label class="c-bp-switch" ng-if="VORModel.VOR_Header.Status != 'Stocked' && VORModel.VOR_Header.Status != 'Invoiced' && VORModel.VOR_Header.VendorName != null">
                                                        <input class="c-slider-input" ng-click="VORModel.onClickReceivingFromInvoice()" ng-model="VORModel.VOR_Header.IsReceivingFromInvoice" type="checkbox" />
                                                        <div class="c-bp-slider" ng-class="{'c-checkd-slider':VORModel.VOR_Header.IsReceivingFromInvoice}"></div>
                                                    </label>
                                                    <label class="c-bp-switch" ng-if="VORModel.VOR_Header.Status == 'Stocked' || VORModel.VOR_Header.Status == 'Invoiced'|| VORModel.VOR_Header.VendorName == null">
                                                        <input class="c-slider-input" ng-click="VORModel.onClickReceivingFromInvoice()" ng-model="VORModel.VOR_Header.IsReceivingFromInvoice" type="checkbox" ng-disabled="VORModel.VOR_Header.Status == 'Stocked' || VORModel.VOR_Header.Status == 'Invoiced'|| VORModel.VOR_Header.VendorName == null" />
                                                        <div class="c-bp-slider disableSlider" ng-class="{'c-checkd-slider':VORModel.VOR_Header.IsReceivingFromInvoice}"></div>
                                                    </label>
                                                    <label>{{'Receiving_from_an_invoice' | translate}}</label>
                                                </div>
                                            </div>
                                            <div class="col-md-12 Plr0 Mrg20">
                                                <div class="generalInfoData">
                                                    <label class="T7 required_field"> {{VORModel.VOR_Header.IsReceivingFromInvoice ?'Label_Invoice_Number' : 'Label_Packing_Slip' | translate}}</label>
                                                </div>
                                                <div class="mycustvalue"
                                                    id="VOR_info_form-label_PACKING_SLIP_selection">
                                                    <input type="text" class="form-control"
                                                        id="packingSlipNumber"
                                                        placeholder="{{VORModel.VOR_Header.IsReceivingFromInvoice ?'Invoice#' : 'Packing Slip#'}}"
                                                        maxlength="255"
                                                        ng-model="VORModel.VOR_Header.PackagingSlipNumber"
                                                        rel="PackingSlipNumberId"
                                                        ng-class="{'redborder' : VORModel.VendorOrderReceivingFormValidationModal['PackagingSlipNumber']['isError'] == true}"
                                                        data-toggle="tooltip"
                                                        title="{{VORModel.VOR_Header['PackagingSlipNumber']['ErrorMessage']}}"
