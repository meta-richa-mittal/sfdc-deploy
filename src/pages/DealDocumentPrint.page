                                                                    <col width="9%" />
                                                                    <col ng-if="!unitDeal.ItemDesc.IsConsignment && !unitDeal.ItemDesc.IsNewUnit" width="18%" />
                                                                    <col ng-if="!unitDeal.ItemDesc.IsConsignment && unitDeal.ItemDesc.IsNewUnit" width="19%" />
                                                                    <col width="15%" />
                                                                    <tbody>
                                                                        <tr class="tableHeader T3 ">
                                                                            <td class="T17">{{unitDeal.ItemDesc.Year}}</td>
