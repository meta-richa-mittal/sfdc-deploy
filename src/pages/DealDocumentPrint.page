                                                                        <col width="32%" />
                                                                        <col width="5%" />
                                                                        <col width="5%" />
                                                                        <col width="5%" />
                                                                        <col width="13%" />
                                                                        <col width="13%" />
                                                                        <col width="14%" />
                                                                        <tbody ng-repeat = "installoptionAndFee in unitDeal.InstalloptionAndFeeList" ng-init="installoptionAndFeeIndex = $index">
