                                                                                <span ng-if="!unitDeal.ItemDesc.IsNewUnit">USED -</span>
                                                                            </td>
                                                                            <td class="T17">{{unitDeal.ItemDesc.UnitName}}</td>
                                                                            <td></td>
                                                                            <td class="T17 Text-center">{{unitDeal.ItemDesc.Colour}}</td>
                                                                            <td class="T17">{{unitDeal.ItemDesc.Mileage}}</td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
