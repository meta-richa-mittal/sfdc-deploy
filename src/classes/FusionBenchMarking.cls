    public void insertLineItems() {
        populateOrderNumberToSourceMap();
        List<Fusion_Report_Line_Item__c> fusionLineItemList = new List<Fusion_Report_Line_Item__c>();
        for (Integer orderNumber : orderNumberToHDNetAccountMap.keySet()) {
            Fusion_Report_Line_Item__c fusionLineItemRec = new Fusion_Report_Line_Item__c();
            fusionLineItemRec.HDNet_Account__c = orderNumberToHDNetAccountMap.get(orderNumber);
            fusionLineItemRec.Map_Action__c = orderNumberToMapActionMap.get(orderNumber);
            fusionLineItemRec.Source__c = orderNumberToSourceMap.get(orderNumber);
            fusionLineItemRec.Order_Number__c = orderNumber;
            fusionLineItemRec.Description__c = orderNumberToModalWindowDescriptionMap.get(orderNumber);
            fusionLineItemList.add(fusionLineItemRec);
        }
        if(fusionLineItemList.size() > 0) {
            insert fusionLineItemList;
        }
    }
    
