    public static Filter__c insertFilter(String  filterLabel, Boolean isFavorite, Boolean isCommon, Boolean isCustom, Boolean isRecociliation, String userId, String filterId) {
        Filter__c filterRec = new Filter__c(Id = filterId);
        filterRec.Filter_Label__c = filterLabel;
        filterRec.CommonFilterCategory__c = FILTER_LABEL_TO_COMMON_FILTER_CATEGORY_MAP.get(filterLabel);
