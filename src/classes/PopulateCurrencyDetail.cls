public without sharing class PopulateCurrencyDetail {

    private static Final String CURRENCY_DETAIL_JSON = BPUtility.getEncodedString('[{"Name":"Canada","CurrencyDetail":{"Code":"CAD","Name":"Dollar","Symbol":"$"},"Flag":""},{"name":"USA","CurrencyDetail":{"code":"USD","name":"Dollar","symbol":"$"},"flag":""},{"name":"Australia","CurrencyDetail":{"code":"AUD","name":"Dollar","symbol":"$"},"flag":""},{"name":"New Zealand","CurrencyDetail":{"code":"NZD","name":"Dollar","symbol":"$"},"flag":""},{"name":"United Kingdom","CurrencyDetail":{"code":"GBP","name":"Pound","symbol":"£"},"flag":""},{"name":"Iraq","CurrencyDetail":{"code":"IQD","name":"Dinar","symbol":"ع.د"},"flag":""}]');

    public PopulateCurrencyDetail() {
        List<Country__c> existingCountryList = [SELECT Id, Name__c FROM Country__c];
        List<CountryDetailWrapper> countryDetailWrapperList = (List<CountryDetailWrapper>) System.JSON.deserialize(BPUtility.getDecodedString(CURRENCY_DETAIL_JSON), List<CountryDetailWrapper>.class);

        populateCurrencyDetailOnCountries(existingCountryList, countryDetailWrapperList);
    }

    private static void populateCurrencyDetailOnCountries(List<Country__c> existingCountryList, List<CountryDetailWrapper> countryDetailWrapperList) {
        Map<String, String> countryNameToIdMap = new Map<String, String>();
        for(Country__c countryRec : existingCountryList) {
            countryNameToIdMap.put(countryRec.Name__c.toLowerCase(), countryRec.Id);
        }

        List<Country__c> countryList = new List<Country__c>();
        Country__c country;
        for(CountryDetailWrapper countryDetailObj : countryDetailWrapperList) {
            country = new Country__c();
            country.Id = countryNameToIdMap.get(countryDetailObj.Name.toLowerCase()) != null ? countryNameToIdMap.get(countryDetailObj.Name.toLowerCase()): null;
            if(String.isBlank(country.Id)) country.Name__c = countryDetailObj.Name;
            country.Flag_Encoded__c = countryDetailObj.Flag;
            country.Flag__c = '<img src="data:image/jpeg;base64,'+ countryDetailObj.Flag +'">';
            country.Currency_Name__c = String.isNotBlank(countryDetailObj.CurrencyDetail.Name) ? countryDetailObj.CurrencyDetail.Name : '' ;
            country.Currency_Code__c = String.isNotBlank(countryDetailObj.CurrencyDetail.Code) ? countryDetailObj.CurrencyDetail.Code : '' ;
            country.Currency_Symbol__c = String.isNotBlank(countryDetailObj.CurrencyDetail.Symbol) ? countryDetailObj.CurrencyDetail.Symbol : '';

            countryList.add(country);
        }
        DMLUtility.upsertSobjectList('Country__c', countryList);
    }

    public class CountryDetailWrapper {
        public String Name;
        public CurrencyDetailWrapper CurrencyDetail;
        public String Flag;
    }

    public class CurrencyDetailWrapper {
        public String Code;
        public String Name;
        public String Symbol;
    }
}